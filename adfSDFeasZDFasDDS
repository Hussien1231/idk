getgenv().RainbowTjm32.t2.MouseButton1Click:Connect(function()
	if game.Players.LocalPlayer.Character.Humanoid.SeatPart.Name ~= "DriveSeat" then
		for _,P in ipairs(game.workspace:GetChildren()) do 
			if P.Name ~= "FreeCars" and P.Name ~= "Money" and P.Name ~= "skins" then
				for _, Desc in ipairs(P:GetDescendants()) do
					if Desc:IsA("BasePart") then
						Desc.CanTouch = true
					end
				end
			end
		end
		getgenv().RainbowTjm32.t2.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		return 
	else
		for _,P in ipairs(game.workspace:GetChildren()) do 
			if P.Name ~= "FreeCars" and P.Name ~= "Money" and P.Name ~= "skins" then
				for _, Desc in ipairs(P:GetDescendants()) do
					if Desc:IsA("BasePart") then
						Desc.CanTouch = false
					end
				end
			end
		end
	end 
	ChangeToggleColor(getgenv().RainbowTjm32.t2)
	if getgenv().RainbowTjm32.t2.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		SendNotify("ملاحظة", "هاذ الامر يطير اي شخص يصدم بسيارتك وانت تسوقها", 10)
		SendNotify("ملاحظة", "اذا واحد صعد معك ونزل بيطير", 10)
		local c, hrp, vel, movel = nil, nil, nil, 0.1
		while getgenv().RainbowTjm32.t2.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) do task.wait()
			game:GetService("RunService").Heartbeat:Wait()
			c = game.Players.LocalPlayer.Character
			hrp = c and c:FindFirstChild("HumanoidRootPart")
			if hrp then
				vel = hrp.Velocity
				hrp.Velocity = vel * 1000 + Vector3.new(0, 1000, 0)
				game:GetService("RunService").RenderStepped:Wait()
				hrp.Velocity = vel
				game:GetService("RunService").Stepped:Wait()
				hrp.Velocity = vel + Vector3.new(0, movel, 0)
				movel = -movel
			end
			for _,P in ipairs(game.Players:GetPlayers()) do 
				if P ~= game.Players.LocalPlayer then
					for _,O in ipairs(P:GetChildren()) do 
						if O:IsA("BasePart") then
							O.CanCollide = false
						end
					end
				end
			end
		end
	end
end)
LoadChatAlert()
else
LoadChatAlert()
end  
