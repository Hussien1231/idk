getgenv().RainbowTjm35.t2.MouseButton1Click:Connect(function()
	pcall(function()
		if TargetedPlayer ~= nil then
			local Cuff = GetCuff()
			if not Cuff then
				SendNotify("System VR7","لازم يكون معك كلبشة",5) return 
			elseif Players[TargetedPlayer].Character.UpperTorso.Anchored then
				SendNotify("System VR7","الاعب متجمد او مكلبش بل اساس",5) return 
			end 
			if Cuff and Cuff.Parent == game.Players.LocalPlayer.Backpack then Cuff.Parent = game.Players.LocalPlayer.Character end
			wait()
			Cuff.RemoteEvent:FireServer(Players[TargetedPlayer].Character.HumanoidRootPart, "Cuff")
			wait(0.2)
			Cuff.RemoteEvent:FireServer(Players[TargetedPlayer].Character.HumanoidRootPart, "Cuff")
		end
	end)
end)

