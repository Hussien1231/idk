getgenv().JerkingAnimationR6.MouseButton1Click:Connect(function()
	if game.Players.LocalPlayer.Character.Humanoid.RigType ~= Enum.HumanoidRigType.R6 then
		SendNotify("System VR7","يجب ان تكون R6",7)
		return
	end
	getgenv().idkjustcheck = false
	local Track
	local Animation = Instance.new("Animation")
	Animation.AnimationId = "rbxassetid://204292303"
	local Animation1 = Instance.new("Animation")
	Animation1.AnimationId = "rbxassetid://181526230"
	for _, Track in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
		if Track.Animation.AnimationId == Animation.AnimationId or Track.Animation.AnimationId == Animation1.AnimationId then
			Track:Stop()
			getgenv().idkjustcheck = true
		else
			Track:Stop()
		end
	end
	if getgenv().idkjustcheck then getgenv().idkjustcheck = false return end 
	local Humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
	Track = Humanoid:LoadAnimation(Animation)
	Track.Priority = Enum.AnimationPriority.Action
	Track.Looped = true
	Track:Play(.1, 1, 3)
	Track1 = Humanoid:LoadAnimation(Animation1)
	Track1.Priority = Enum.AnimationPriority.Action
	Track1.Looped = true
	Track1:Play(.1, 1, 1)
end)

