			if tt >= 5 then
				tt = 0
				game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eColo1r"):FireServer("PickingRPNameColor",Color3.new(math.random(0,1),math.random(0,1),math.random(0,1)))
