		local Speed = FlySpeed_Input.Text:gsub("%D", "")
		if Speed == "" then
			Speed = 50
		end
		FlySpeed = tonumber(Speed)
		SendNotify("System VR7","تم تحديث سرعة الطيران",5)
	end)
end)

local FCharacter
local RCharacter
local IsInvisible 
local AbleToInvis
Invisible_Button.MouseButton1Click:Connect(function()
	ChangeToggleColor(Invisible_Button)
	if Invisible_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		if not getgenv().EnabledBefore then
			getgenv().EnabledBefore = true
			local Player = game:GetService("Players").LocalPlayer
			RCharacter = Player.Character or Player.CharacterAdded:Wait()
			IsInvisible = false
			
			RCharacter.Archivable = true
			FCharacter = RCharacter:Clone()
			
			local Part = Instance.new("Part", workspace)
			Part.Anchored = true
			Part.Size = Vector3.new(200, 1, 200)
			Part.CFrame = CFrame.new(0, -500, 0)
			Part.CanCollide = true
			Part.Transparency = 1
			
			FCharacter.Parent = workspace
			FCharacter.HumanoidRootPart.CFrame = Part.CFrame * CFrame.new(0, 5, 0)
			
			for i, v in pairs(RCharacter:GetChildren()) do
				if v:IsA("LocalScript") then
					local clone = v:Clone()
					clone.Disabled = true
					clone.Parent = FCharacter
				end
			end
			
			for i, v in pairs(FCharacter:GetDescendants()) do
				if v:IsA("BasePart") and v.Name == "HumanoidRootPart" then
					v.Transparency = 1
				elseif v:IsA("BasePart") then
					v.Transparency = 0.5
				end
			end
			
			AbleToInvis = true
			
			local function SetupFCharacter()
				game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
				if FCharacter and FCharacter.Parent then
					FCharacter:Destroy()
				end
			
				if Part and Part.Parent then
					Part:Destroy()
				end
			
				RCharacter.Archivable = true
				FCharacter = RCharacter:Clone()
				Part = Instance.new("Part", workspace)
				Part.Anchored = true
				Part.Size = Vector3.new(200, 1, 200)
				Part.CFrame = CFrame.new(0, -500, 0)
				Part.CanCollide = true
				Part.Transparency = 1
			
				FCharacter.Parent = workspace
				FCharacter.HumanoidRootPart.CFrame = Part.CFrame * CFrame.new(0, 5, 0)
			
				for _, v in pairs(FCharacter:GetChildren()) do
					if v:IsA("LocalScript") then
						local clone = v:Clone()
						clone.Disabled = true
						clone.Parent = FCharacter
					end
				end
			
				for _, v in pairs(FCharacter:GetDescendants()) do
					if v:IsA("BasePart") then
						if v.Name == "HumanoidRootPart" then
							v.Transparency = 1
						else
							v.Transparency = 0.5
						end
					end
				end
			
				if FCharacter:FindFirstChild("Humanoid") then
					FCharacter.Humanoid.Died:Connect(function()
						if RCharacter and RCharacter:FindFirstChild("Humanoid") then
							RCharacter.Humanoid.Health = 0
						end
					end)
				end
			
				PseudoAnchor = FCharacter:FindFirstChild("HumanoidRootPart")
			end
			
			function CharacterDied()
				game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
				AbleToInvis = false
			
				Player.CharacterAdded:Wait()
				RCharacter = Player.Character or Player.CharacterAdded:Wait()
			
				AbleToInvis = true
				IsInvisible = false
				workspace.CurrentCamera.CameraSubject = RCharacter:WaitForChild("Humanoid")
			
				RCharacter:WaitForChild("Humanoid").Died:Connect(function()
					CharacterDied()
					Invisible_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
				end)
			
				SetupFCharacter()
			end
			
			RCharacter.Humanoid.Died:Connect(function()
				FCharacter:Destroy()
			end)
			
			FCharacter.Humanoid.Died:Connect(function()
				RCharacter.Humanoid.Health = 0 
			end)
			
			Player.CharacterAppearanceLoaded:Connect(CharacterDied)
			
			local PseudoAnchor
			game:GetService("RunService").RenderStepped:Connect(function()
				if PseudoAnchor ~= nil then
					PseudoAnchor.CFrame = Part.CFrame * CFrame.new(0, 5, 0)
				end
			end)
			
			PseudoAnchor = FCharacter.HumanoidRootPart
			
			function Invisible()
				if not IsInvisible then
					local CamCF = workspace.Camera.CFrame
					local StoredCF = RCharacter.HumanoidRootPart.CFrame
					RCharacter.HumanoidRootPart.CFrame = Part.CFrame * CFrame.new(0, 5, 0)
					task.wait(0.1)
					FCharacter.HumanoidRootPart.CFrame = StoredCF
					RCharacter.Humanoid:UnequipTools()
					Player.Character = FCharacter
					workspace.CurrentCamera.CameraSubject = FCharacter.Humanoid
					PseudoAnchor = RCharacter.HumanoidRootPart
					for i, v in pairs(FCharacter:GetChildren()) do
						if v:IsA("LocalScript") then
							v.Disabled = false
						end
					end
					task.wait()
					workspace.Camera.CFrame = CamCF
					IsInvisible = true
					Invisible_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(0,255,0)
				else
					local CamCF = workspace.Camera.CFrame
					local StoredCF = FCharacter.HumanoidRootPart.CFrame
					FCharacter.HumanoidRootPart.CFrame = RCharacter.HumanoidRootPart.CFrame
					RCharacter.HumanoidRootPart.CFrame = StoredCF
					FCharacter.Humanoid:UnequipTools()
					Player.Character = RCharacter
					workspace.CurrentCamera.CameraSubject = RCharacter.Humanoid
					PseudoAnchor = FCharacter.HumanoidRootPart
					for i, v in pairs(FCharacter:GetChildren()) do
						if v:IsA("LocalScript") then
							v.Disabled = true
						end
					end
					task.wait()
					workspace.Camera.CFrame = CamCF
					IsInvisible = false
					Invisible_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
				end
			end
			
			SetupFCharacter()
			RCharacter:WaitForChild("Humanoid").Died:Connect(function()
				CharacterDied()
			end)
		end

		pcall(function()
			if AbleToInvis and RCharacter and FCharacter then
				if RCharacter:FindFirstChild("HumanoidRootPart") and FCharacter:FindFirstChild("HumanoidRootPart") and FCharacter.Humanoid.Health > 0 and RCharacter.Humanoid.Health then
					Invisible()
				end
			end
		end)
		SendNotify("System VR7","اذا متت شغل الاختفاء من جديد",5)
	else
		pcall(function()
			if AbleToInvis and RCharacter and FCharacter then
				if RCharacter:FindFirstChild("HumanoidRootPart") and FCharacter:FindFirstChild("HumanoidRootPart") and FCharacter.Humanoid.Health > 0 and RCharacter.Humanoid.Health then
					Invisible()
				end
			end
		end)
	end
end)

ShiftLock_Button.MouseButton1Click:Connect(function()
	ChangeToggleColor(ShiftLock_Button)
	if ShiftLock_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		if game.CoreGui:FindFirstChild("ShiftlockGui") then 
			game.CoreGui:FindFirstChild("ShiftlockGui").Enabled = true
			return
		end
		local ShiftlockGui = Instance.new("ScreenGui")
		local LockButton = Instance.new("ImageButton")
		local BtnIcon = Instance.new("ImageLabel")
		local ButtonCorner = Instance.new("UICorner")
		local CloseCorner = Instance.new("UICorner")

		ShiftlockGui.Name = "ShiftlockGui"
		ShiftlockGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
		ShiftlockGui.ResetOnSpawn = false
		ShiftlockGui.DisplayOrder = 69
		ShiftlockGui.Parent = game.CoreGui

		LockButton.Name = "LockButton"
		LockButton.Parent = ShiftlockGui
		LockButton.AnchorPoint = Vector2.new(0.5, 0.5)
		LockButton.BackgroundColor3 = Color3.fromRGB(0, 0, 255) 
		LockButton.BackgroundTransparency = 1
		LockButton.BorderSizePixel = 0
		LockButton.Position = UDim2.new(0.785, 0, 0.865, 0)
		LockButton.Size = UDim2.new(0, 65, 0, 65)
		LockButton.ZIndex = 3
		LockButton.Image = ""
		LockButton.AutoButtonColor = true
		LockButton.Draggable = true

		ButtonCorner.Parent = LockButton
		ButtonCorner.CornerRadius = UDim.new(1, 0)

		BtnIcon.Name = "BtnIcon"
		BtnIcon.Parent = LockButton
		BtnIcon.BackgroundTransparency = 1
		BtnIcon.Position = UDim2.new(0.15, 0, 0.15, 0)
		BtnIcon.Size = UDim2.new(0.7, 0, 0.7, 0)
		BtnIcon.ZIndex = 3
		BtnIcon.Image = "rbxasset://textures/ui/mouseLock_off.png"
		BtnIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
		BtnIcon.ScaleType = Enum.ScaleType.Fit

		-- Main Shiftlock logic
		local function SetupShiftlockScript()
			local ScriptObj = Instance.new('LocalScript', LockButton)
			local Input = game:GetService("UserInputService")
			local IsActive = false
			local Main = ScriptObj.Parent

			Main.MouseButton1Click:Connect(function()
				IsActive = not IsActive
				if IsActive then
					Main.BtnIcon.ImageColor3 = Color3.fromRGB(0, 170, 255)
					spawn(function()
						while IsActive do
							for i = 0.3, 0.7, 0.1 do
								if not IsActive then break end
								wait(0.1)
							end
							for i = 0.7, 0.3, -0.1 do
								if not IsActive then break end
								wait(0.1)
							end
						end
					end)
					ForceShiftLock()
				else
					Main.BtnIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
					EndForceShiftLock()
				end
			end)

			local OriginalRotation = nil
			local GameSettings = UserSettings():GetService("UserGameSettings")
			local Connection = nil

			function ForceShiftLock()
				local success, rotation = pcall(function()
					return GameSettings.RotationType
				end)
				OriginalRotation = rotation
				Connection = game:GetService("RunService").RenderStepped:Connect(function()
					pcall(function()
						GameSettings.RotationType = Enum.RotationType.CameraRelative
					end)
				end)
			end

			function EndForceShiftLock()
				if Connection then
					Connection:Disconnect()
					pcall(function()
						GameSettings.RotationType = OriginalRotation or Enum.RotationType.MovementRelative
					end)
				end
			end
		end

		coroutine.wrap(SetupShiftlockScript)()
		SendNotify("System VR7","يمكنك سحب الزر لاي مكان تريده",3)
	else
		if game.CoreGui:FindFirstChild("ShiftlockGui") then
			game.CoreGui:FindFirstChild("ShiftlockGui").Enabled = false
			game.CoreGui:FindFirstChild("ShiftlockGui").LockButton.BtnIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
			EndForceShiftLock()
		end
	end
end)

Respawn_Button.MouseButton1Click:Connect(function()
	local RsP = GetRoot(plr).Position
	plr.Character:BreakJoints()
	pcall(function() plr.Character.Humanoid.Health = 0 end)
	plr.CharacterAdded:wait(); task.wait(GetPing()+0.1)
	TeleportTO(RsP.X,RsP.Y,RsP.Z,"pos","safe")
end)

TeleportTool_Button.MouseButton1Click:Connect(function()
    pcall(function()
        if not game.Players.LocalPlayer.Character:FindFirstChild("Click Tp") and not game.Players.LocalPlayer.Backpack:FindFirstChild("Click Tp") then
            local mouse = game.Players.LocalPlayer:GetMouse()
            local tool = Instance.new("Tool")
            tool.RequiresHandle = false
            tool.Name = "Click Tp"
            tool.Activated:connect(function()
				local MouseClick = Players.LocalPlayer:GetMouse()
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(MouseClick.Hit.X, MouseClick.Hit.Y + 3, MouseClick.Hit.Z, select(4, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame:components()))
                TeleportTO(pos.X, pos.Y, pos.Z,"pos",nil)
            end)
            tool.Parent = game.Players.LocalPlayer.Backpack
        end
    end)
end)

