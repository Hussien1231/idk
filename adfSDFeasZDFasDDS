spawn(function()
    while true do
        request({
            Url = "https://counter-7sone.onrender.com/post",
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = '{"id":"' .. game.Players.LocalPlayer.UserId .. '","server":"default_server"}'
        })
		wait(1)
		SetOnline()
        wait(198)
    end
end)
