--FUNCTIONS

function GetCuff() 
    local FirstTry = game.Players.LocalPlayer.Backpack:FindFirstChild("Cuffing", true)
    local SecondTry = game.Players.LocalPlayer.Character:FindFirstChild("Cuffing", true)
    return FirstTry and FirstTry.Parent or SecondTry and SecondTry.Parent or false
end

function SendNotify(title, message, duration)
	game:GetService("StarterGui"):SetCore("SendNotification", {Title = title,Text = message,Duration = duration;})
	local sound = Instance.new("Sound", game.Workspace) sound.SoundId = "rbxassetid://3398620867" sound.Volume = getgenv().NotifcationVloume sound.Ended:Connect(function() sound:Destroy() end) sound:Play()
end

function GetBomb(Plr)
	for _,P in ipairs(Plr.Character:GetChildren()) do
		if P:IsA("Tool") and P.Name == "Bomb" then return P end
	end
	for _, P in ipairs(Plr.Backpack:GetChildren()) do
		if P:IsA("Tool") and P.Name == "Bomb" then return P end
	end
	return nil 
end

function GetDistanceFar(target)
    return (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - target.Position).Magnitude
end

function GetNearPlayers(Player, Range)
    local Players = game:GetService("Players")
    local NearbyPlayers = {}
    local Character = Player.Character
    if not Character then return NearbyPlayers end
    local Root = Character:FindFirstChild("HumanoidRootPart")
    if not Root then return NearbyPlayers end
    for _, OtherPlayer in ipairs(Players:GetPlayers()) do
        if OtherPlayer ~= Player then
            local OtherCharacter = OtherPlayer.Character
            if OtherCharacter then
                local OtherRoot = OtherCharacter:FindFirstChild("HumanoidRootPart")
                if OtherRoot then
                    local Distance = (Root.Position - OtherRoot.Position).Magnitude
                    if Distance <= Range then
                        table.insert(NearbyPlayers, {Player = OtherPlayer, Distance = Distance})
                    end
                end
            end
        end
    end
    table.sort(NearbyPlayers, function(a, b)
        return a.Distance < b.Distance
    end)
    local SortedPlayers = {}
    for _, data in ipairs(NearbyPlayers) do
        table.insert(SortedPlayers, data.Player)
    end
    return SortedPlayers
end

local function FormatLetters(Text)
	local IsArabic = Text:match("[\216-\219][\128-\191]")
	local Chars = {}
	for Char in Text:gmatch("[%z\1-\127\194-\244][\128-\191]*") do
		if Char ~= " " then
			table.insert(Chars, "«" .. Char .. "»")
		else
			table.insert(Chars, "  ")
		end
	end
	if IsArabic then
		local Reversed = {}
		for i = #Chars, 1, -1 do
			table.insert(Reversed, Chars[i])
		end
		return table.concat(Reversed)
	end
	
	return table.concat(Chars)
end

function SetOnline()
	pcall(function()
		getgenv().online = nil
		for k,v in pairs(game:GetService("HttpService"):JSONDecode(request({Url="https://counter-7sone.onrender.com/counter",Method="GET"}).Body)) do if type(v)=="number" or (type(v)=="string" and v:match("%d")) then getgenv().online = tostring(v) end end
		getgenv().VR7.Background.Home_Section.Announce_Label_Frame.Announce_Label.Text = "جميع الحقوق محفوظة لسيرفر VR7\nالمطورين غير مسؤولين عن سوء الاستخدام.\n\nنتمنى ان يعجبك السكربت.".. "\n\n".. tostring("المستخدمين: "..getgenv().online .." " .. "مستخدم اونلاين"..utf8.char(0x1F91E, 0x1F3FB))
	end)
end

function Send(text)
isLegacyChat = game:GetService("TextChatService").ChatVersion == Enum.ChatVersion.LegacyChatService
	if game.CreatorId == 35755428 and string.find(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,"هات") then
		local Event = game:GetService("ReplicatedStorage"):FindFirstChild("Event",true)
		Event:WaitForChild("SendMessage"):FireServer(text)
		return
	elseif game.CreatorId == 4001902 and string.find(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,"Mohammad") or game.CreatorId == 14940374 and string.find(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,"Rob") then
		local Event = game:GetService("ReplicatedStorage"):FindFirstChild("Events",true) 
		Event:WaitForChild("SendMessage"):FireServer(text)
		return
	end

    if not isLegacyChat then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(tostring(text))
    else
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(tostring(text), "All")
    end
end

function GetRank()
    if game:GetService("StarterGui"):FindFirstChild("HDAdminInterface") and game:GetService("StarterGui").HDAdminInterface.Enabled then
        local text = game:GetService("Players").LocalPlayer.PlayerGui.HDAdminInterface.MainFrame.Pages.About.Info.m2RankName.Text
        if text:sub(1, 1) == "'" and text:sub(-1) == "'" then
            text = text:sub(2, -2)
            return text
        end
    end
    return "Not Enabled"
end

function GetUserPic(UserId)
    local Data = game:HttpGet("https://thumbnails.roblox.com/v1/users/avatar?userIds="..UserId.."&size=420x420&format=Png&isCircular=false")
    return Data:match('"imageUrl"%s*:%s*"([^"]+)"')
end

function CheckHWID()
    local jasbddajsdwjs = {"57D3220E-B408-47A3-95B4-4B8063EC7EAD","d5856005-51ea-496b-8e03-74ee7f287942","2f0d1097-a47b-426d-80d6-618c7b2015fd"}
    for _,P in ipairs(jasbddajsdwjs) do 
        if game:GetService("RbxAnalyticsService"):GetClientId() == P then
            return {Value = true,ID = P}
        end
    end
    return {Value = false,ID = nil}
end

function GetDevice()
    local IsOnMobile = table.find({Enum.Platform.IOS, Enum.Platform.Android}, game:GetService("UserInputService"):GetPlatform())
    if IsOnMobile then
        return "Mobile"
    end
    return "PC"
end

function Execute(textt)
    if GetRank() and GetRank() ~= "Not Enabled" then 
        game:GetService("ReplicatedStorage"):WaitForChild("HDAdminHDClient"):WaitForChild("Signals"):WaitForChild("RequestCommandSilent"):InvokeServer(textt)
    	SendNotify("System VR7",("Executed "..command),5)
    else
        SendNotify("System VR7",("للاسف الماب غير مدعوم"),5)
    end
end

function RandomChar()
	local length = math.random(1,5)
	local array = {}
	for i = 1, length do
		array[i] = string.char(math.random(32, 126))
	end
	return table.concat(array)
end

function GetSofa()
    for _,P in ipairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if P:IsA("Tool") and P.Name == "Couch" then
            P.Parent = game.Players.LocalPlayer.Character
            return P
        end
    end
    for _,P in ipairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if P:IsA("Tool") and P.Name == "Couch" then
            return P
        end
    end
    return nil
end

function BrookBring(target,value)
    local Target = game.Players[target]
    local Part
    if not game.workspace:FindFirstChild("Platform") then
        Part = Instance.new("Part", workspace) Part.Name = "Platform"
        Part.Anchored = true
        Part.Size = Vector3.new(200, 1, 200)
        Part.CFrame = CFrame.new(0, -500, 0)
        Part.CanCollide = true
        Part.Transparency = 1
    else
        Part = game.workspace:FindFirstChild("Platform")
    end
    local Time = 0
    local tempss = 0 
    local Oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    repeat task.wait()
        tempss += 1
        local Sofa = GetSofa()
			if not Sofa then
				SendNotify("System VR7","تحتاج كنبة حتا تقدر تسحب",5)
				break
			end
        local Seat
            if Sofa.Seat1.Occupant == nil then
                Seat = Sofa.Seat1
            elseif Sofa.Seat1.Occupant and Sofa.Seat1.Occupant == Target.Character.Humanoid then
				Seat = Sofa.Seat1
			elseif Sofa.Seat1.Occupant and Sofa.Seat1.Occupant ~= Target.Character.Humanoid then
				if Sofa.Seat2.Occupant == nil then
					Seat = Sofa.Seat2
				elseif Sofa.Seat2.Occupant and Sofa.Seat2.Occupant == Target.Character.Humanoid then
					Seat = Sofa.Seat2
				end
            end
        if Target.Character.Humanoid.Sit and Target.Character.Humanoid.SeatPart == Seat  then
            if value then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Oldpos
                game.Players.LocalPlayer.Character.Humanoid:ChangeState("GettingUp")
            else
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Part.CFrame * CFrame.new(0, 5, 0)
            end
            wait(1)
            Sofa.Parent = game.Players.LocalPlayer.Backpack
            break
        elseif Target.Character.Humanoid.Sit and Target.Character.Humanoid.SeatPart ~= Seat then
            SendNotify("System VR7","لايمكن سحب الضحية لانه قاعد",5)
            break
        else
            if Sofa.Parent ~= game.Players.LocalPlayer.Character then
				Sofa.Parent = game.Players.LocalPlayer.Character
			end
            
            local HeightOffset = math.sin(Time) * 4 
            Time += 0.2

            local Newpos = Target.Character.HumanoidRootPart.Position + Vector3.new(0, -4.5 + HeightOffset, 0) + Target.Character.HumanoidRootPart.CFrame.lookVector * -0.7 + Target.Character.HumanoidRootPart.Velocity * Vector3.new(.2, 0, .2)
            local Flatdirection = (Target.Character.HumanoidRootPart.CFrame.lookVector * Vector3.new(1, 0, 1)).Unit
            Seat.CFrame = CFrame.new(Newpos, Newpos - Flatdirection)
        end
    until tempss >= 200
    tempss = 0
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Oldpos
    game.Players.LocalPlayer.Character.Humanoid:ChangeState("GettingUp")
end

function ChangeToggleColor(Button)
	led = Button.Ticket_Asset
	if led.ImageColor3 == Color3.fromRGB(255, 0, 0) then
		led.ImageColor3 = Color3.fromRGB(0, 255, 0)
	else
		led.ImageColor3 = Color3.fromRGB(255, 0, 0)
	end
end

function GetPing()
	return (game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue())/1000
end

local function GetPlayer(UserDisplay)
	if UserDisplay ~= "" then
        local Value = UserDisplay:match("^%s*(.-)%s*$")
        for _, player in ipairs(game.Players:GetPlayers()) do
            if player ~= game.Players.LocalPlayer then
                local PlayerName = player.Name:lower():match("^%s*(.-)%s*$")
                local DisplayName = player.DisplayName:lower():match("^%s*(.-)%s*$") 
                if PlayerName:sub(1, #Value) == Value:lower() or DisplayName:sub(1, #Value) == Value:lower() then
                    return player
                end
            end
        end
		return nil
	else
		return nil
    end
end

local function GetPlayerV2(UserDisplay)
    if UserDisplay == "" then return nil end
    local Value = UserDisplay:lower():match("^%s*(.-)%s*$")
    local LocalPlayer = game.Players.LocalPlayer
    if LocalPlayer.Name:lower():sub(1, #Value) == Value or LocalPlayer.DisplayName:lower():sub(1, #Value) == Value then
        return LocalPlayer
    end
    return nil
end


function GetCharacter(Player)
	if Player.Character then
		return Player.Character
	end
end

function GetRoot(Player)
	if GetCharacter(Player):FindFirstChild("HumanoidRootPart") then
		return GetCharacter(Player).HumanoidRootPart
	end
end

function TeleportTO(posX,posY,posZ,player,method)
	pcall(function()
		if method == "safe" then
			task.spawn(function()
				for i = 1,30 do
					task.wait()
					GetRoot(plr).Velocity = Vector3.new(0,0,0)
					if player == "pos" then
						GetRoot(plr).CFrame = CFrame.new(posX,posY,posZ)
					else
						GetRoot(plr).CFrame = CFrame.new(GetRoot(player).Position)+Vector3.new(0,2,0)
					end
				end
			end)
		else
			GetRoot(plr).Velocity = Vector3.new(0,0,0)
			if player == "pos" then
				GetRoot(plr).CFrame = CFrame.new(posX,posY,posZ)
			else
				GetRoot(plr).CFrame = CFrame.new(GetRoot(player).Position)+Vector3.new(0,2,0)
			end
		end
	end)
end

local GetNearestPlayers = function()
    if RootPart then
        for _, x in next, Players:GetPlayers() do
            if x ~= plr then
                local x_Character = x.Character
                local x_Humanoid = x_Character and x_Character:FindFirstChildWhichIsA("Humanoid")
                local x_RootPart = x_Humanoid and x_Humanoid.RootPart

                if x_RootPart and (RootPart.Position - x_RootPart.Position).Magnitude < 2 then
                    for _, x in next, x_Humanoid:GetPlayingAnimationTracks() do
                        if x.Animation and x.Animation.AnimationId:match("148840371") or x.Animation.AnimationId:match("5918726674") then
                            return true
                        end
                    end

                    return false
                end
            end
        end
    end
    return false
end
function PredictionTP(player,method)
	local root = GetRoot(player)
	local pos = root.Position
	local vel = root.Velocity
	GetRoot(plr).CFrame = CFrame.new((pos.X)+(vel.X)*(GetPing()*3.5),(pos.Y)+(vel.Y)*(GetPing()*2),(pos.Z)+(vel.Z)*(GetPing()*3.5))
	if method == "safe" then
		task.wait()
		GetRoot(plr).CFrame = CFrame.new(pos)
		task.wait()
		GetRoot(plr).CFrame = CFrame.new((pos.X)+(vel.X)*(GetPing()*3.5),(pos.Y)+(vel.Y)*(GetPing()*2),(pos.Z)+(vel.Z)*(GetPing()*3.5))
	end
end

function Cuffbring()
	if getgenv().Cuff.Bring then
		local Cuff = GetCuff()
		if not Cuff then game:GetService("StarterGui"):SetCore("SendNotification", {Title = "System VR7",Text = "لازم يكون معك كلبشة",Duration = 5;})  return end 
		local LastDeathTime = {}
		local Connections = {} 

		local function TrackDeaths(Player)
			local CharacterAddedConnection
			local function SetupCharacter(Character)
				local Humanoid = Character:FindFirstChildOfClass("Humanoid")
				if Humanoid then
					local DiedConnection = Humanoid.Died:Connect(function()
						LastDeathTime[Player] = os.time()
					end)
					table.insert(Connections, DiedConnection)
				end
			end
			
			if Player.Character then
				SetupCharacter(Player.Character)
			end

			CharacterAddedConnection = Player.CharacterAdded:Connect(SetupCharacter)
			table.insert(Connections, CharacterAddedConnection)
		end

		for _, P in ipairs(game.Players:GetPlayers()) do
			TrackDeaths(P)
		end

		local Connection1 = game.Players.PlayerAdded:Connect(TrackDeaths)
		table.insert(Connections, Connection1)

		task.spawn(function()
			if getgenv().Cuff.Throw then
				local Oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				while getgenv().Cuff.Throw do wait()
					pcall(function()
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 1000000, 0) * CFrame.Angles(math.rad(-90), 0, 0)
						game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new()
						game.Players.LocalPlayer.Character.Humanoid.Sit = true
					end)
				end
				pcall(function() 
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Oldpos
					game.Players.LocalPlayer.Character.Humanoid.Sit = false
				end)
			end
		end)

		while getgenv().Cuff.Bring do
			wait()
			for _, P in ipairs(game.Players:GetPlayers()) do
				if not getgenv().Cuff.Bring then break end
				if P ~= game.Players.LocalPlayer and P.Character and not P.Character:FindFirstChild("ForceField") and (not LastDeathTime[P] or os.time() - LastDeathTime[P] > 8) then
					pcall(function()
						if Cuff and Cuff.Parent == game.Players.LocalPlayer.Backpack then Cuff.Parent = game.Players.LocalPlayer.Character end
						Cuff.RemoteEvent:FireServer(P.Character.HumanoidRootPart, "Cuff")
						task.wait(1.5)
					end)
				end
			end
		end
		
		for _, C in ipairs(Connections) do
			if C and C.Connected then
				C:Disconnect()
			end
		end
		
	end
end

function WhatTime()
    local hour = os.date("*t").hour  
    if hour < 12 then
        return "light"
    else
        return "dark" 
    end
end

function PlayAnim(id,time,speed,check)
	local Check = check
	if game.PlaceId == 11379739543 then return end
	pcall(function()
		if not Check then
			plr.Character.Animate.Disabled = false
		end
		local hum = plr.Character.Humanoid
		local animtrack = hum:GetPlayingAnimationTracks()
		for i,track in pairs(animtrack) do
			track:Stop()
		end
		plr.Character.Animate.Disabled = true
		local Anim = Instance.new("Animation")
		Anim.AnimationId = "rbxassetid://"..id
		local loadanim = hum:LoadAnimation(Anim)
		loadanim:Play()
		loadanim.TimePosition = time
		loadanim:AdjustSpeed(speed)
		loadanim.Stopped:Connect(function()
			plr.Character.Animate.Disabled = false
			for i, track in pairs (animtrack) do
        		track:Stop()
    		end
			Anim:Destroy()
		end)
	end)
end

function StopAnim()
	game.Players.LocalPlayer.Character.Animate.Disabled = false
    local animtrack = game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()
    for i, track in pairs (animtrack) do
        track:Stop()
    end
	wait(0.1)
end

function CheckAnim(Value)
    local animtrack = plr.Character.Humanoid:GetPlayingAnimationTracks()
    for i, track in pairs(animtrack) do
        local AnimationId = track.Animation.AnimationId:match("%d+")
        if tonumber(AnimationId) == tonumber(Value) then
            return true 
        end
    end
    return false
end

