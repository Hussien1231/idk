	end
end)

FocusTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(FocusTarget_Button)
		FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BenxTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)

		if FocusTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
            if game.PlaceId == 11379739543 then SendNotify("V R 7","هاذ الماب محمي من هذه الاوامر",3) HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) return end
			getgenv().oldcf = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
			SendNotify("System VR7","الخيار ذا يسمحلك انك تسمع الشخص وهو يسمعك لما تتكلم مايك",3)
			repeat task.wait()
				GetRoot(plr).CFrame = Players[TargetedPlayer].Character.Head.CFrame * CFrame.new(0, -20, 0) * CFrame.Angles(math.rad(180), 0, 0)
				game.Workspace.CurrentCamera.CameraSubject = Players[TargetedPlayer].Character.Humanoid
			until FocusTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
			game.Workspace.CurrentCamera.CameraSubject = GetRoot(plr).Parent.Humanoid
			TeleportTO(getgenv().oldcf.X,getgenv().oldcf.Y,getgenv().oldcf.Z,"pos","safe")
            game.Players.LocalPlayer.Character.Humanoid:ChangeState("GettingUp")
		end
	end
end)

BenxTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(BenxTarget_Button)
		if BenxTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then 
			FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			game:GetService("StarterGui"):SetCore("SendNotification", {Title = "System VR7",Text = "يمكنك تغير سرعة الاغتصاب من الاعدادات في الواجهه",Duration = 3;})
		if game.PlaceId ~= 11379739543 then
			repeat
				pcall(function()
					local OtherTorso = Players[TargetedPlayer].Character:FindFirstChild("Torso") or Players[TargetedPlayer].Character:FindFirstChild("UpperTorso")
					if GetRoot(plr).Parent.Humanoid.RigType.Name == "R15" and GetRoot(plr).Parent.Humanoid.Sit  then if not CheckAnim("5918726674") then PlayAnim(5918726674,0,tonumber(getgenv().ConfigData.BangSpeed)) end elseif GetRoot(plr).Parent.Humanoid.Sit then  if  not CheckAnim("148840371") then PlayAnim(148840371,0,tonumber(getgenv().ConfigData.BangSpeed+1.7)) end end
                    GetRoot(plr).Parent.Humanoid.Sit = true 
					GetRoot(plr).CFrame = OtherTorso.CFrame * CFrame.new(0, 0, 1)
					GetRoot(plr).Velocity = Vector3.new(0,0,0)
				end)
				workspace.FallenPartsDestroyHeight = 0 / 0
				task.wait()
			until BenxTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
		else
			repeat
				pcall(function()
					local targetChar = Players[TargetedPlayer].Character
					if targetChar and GetRoot(plr) then
						local torso = targetChar:FindFirstChild("Torso") or targetChar:FindFirstChild("UpperTorso")
						if torso then
							local root = GetRoot(plr)
							root.Velocity = Vector3.new()
							local offset = math.sin(tick()*20)*0.6
							root.CFrame = torso.CFrame * CFrame.new(0, 0, 2 + offset)
						end
					end
				end)
				workspace.FallenPartsDestroyHeight = 0/0
				task.wait()
			until BenxTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
		end
			workspace.FallenPartsDestroyHeight = -500
            StopAnim()
			GetRoot(plr).Parent.Humanoid.Sit = false
		end
	end
end)

HeadsitTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(HeadsitTarget_Button)
		if HeadsitTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			if game.PlaceId == 11379739543 then SendNotify("V R 7","هاذ الماب محمي من هذه الاوامر",3) HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) return end
			FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			BenxTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			repeat
				pcall(function()
					local targethead = Players[TargetedPlayer].Character.Head
					plr.Character.Humanoid.Sit = true
					GetRoot(plr).CFrame = targethead.CFrame * CFrame.new(0,2,0)
					GetRoot(plr).Velocity = Vector3.new(0,0,0)
				end)
				task.wait()
			until HeadsitTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
			GetRoot(plr).Parent.Humanoid.Sit = false
		end
	end
end)

StandTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(StandTarget_Button)
		FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BenxTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)

		if StandTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			if game.PlaceId == 11379739543 then SendNotify("V R 7","هاذ الماب محمي من هذه الاوامر",3) StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) return end
			for _, Track in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
				Track:Stop()
			end
            local PositionValue = -2
			local increment = 0.3
			repeat task.wait(0.01)
				pcall(function()
                    GetRoot(plr).Parent.Humanoid.Sit = true
					GetRoot(plr).CFrame = Players[TargetedPlayer].Character.Head.CFrame * CFrame.new(0, 0.7, PositionValue) * CFrame.Angles(0, math.rad(180), 0)
					GetRoot(plr).Velocity = Vector3.new(0,0,0)
                  PositionValue = PositionValue + increment
					if PositionValue >= -1 or PositionValue <= -2 then
						increment = -increment
					end
				end)
			until StandTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
			GetRoot(plr).Parent.Humanoid.Sit = false
		end
	end
end)

BackpackTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(BackpackTarget_Button)
		FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BenxTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)

		if BackpackTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			if game.PlaceId == 11379739543 then SendNotify("V R 7","هاذ الماب محمي من هذه الاوامر",3) BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) return end
			repeat
				pcall(function()
					local root = GetRoot(Players[TargetedPlayer])
					plr.Character.Humanoid.Sit = true
					GetRoot(plr).CFrame = root.CFrame * CFrame.new(0,0,1.2) * CFrame.Angles(0, -3, 0)
					GetRoot(plr).Velocity = Vector3.new(0,0,0)
				end)
				task.wait()
			until BackpackTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
			GetRoot(plr).Parent.Humanoid.Sit = false
		end
	end
end)

JerkOnTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(JerkOnTarget_Button)
		FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BenxTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)

		if JerkOnTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			if game.PlaceId == 11379739543 then SendNotify("V R 7","هاذ الماب محمي من هذه الاوامر",3) JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) return end
            spawn(function()
				workspace.FallenPartsDestroyHeight = 0/0
				while JerkOnTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) do 
					pcall(function()
						if game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
							if game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
								PlayAnim(72042024,0.675,1,true)
								wait(0.4)
								elseif game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
								PlayAnim(698251653,0.6,0.4,true)
								task.wait(0.16)
							end
						end
					end)
					task.wait()
                end
                StopAnim()
            end)
			repeat
				pcall(function()
					if game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
						local root = GetRoot(Players[TargetedPlayer])
						game.Players.LocalPlayer.Character.Humanoid:ChangeState("GettingUp")
                        if not game.Players.LocalPlayer.Character.Humanoid.Sit then game.Players.LocalPlayer.Character.Humanoid.Sit = true end 
						GetRoot(plr).CFrame = root.CFrame * CFrame.new(0, 1, -3) * CFrame.Angles(0, math.pi, 0)
						GetRoot(plr).Velocity = Vector3.new(0,0,0)
					end
				end)
				task.wait()
			until JerkOnTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
			workspace.FallenPartsDestroyHeight = -500
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
		end
	end
end)

RepeatTalk_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(RepeatTalk_Button)
		if RepeatTalk_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			TalkRepeater = game:GetService("TextChatService").MessageReceived:Connect(function(msg)
			if msg.TextSource and msg.TextSource.UserId == Players[TargetedPlayer].UserId then
				Send(msg.Text)
			end
		end)
		else
			if TalkRepeater then
				TalkRepeater:Disconnect()
			end
		end
	end
end)
TeleportTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		TeleportTO(0,0,0,Players[TargetedPlayer],nil)
	end
end)

TargetName_Input.FocusLost:Connect(function()
	local LabelText = TargetName_Input.Text
	local LabelTarget = GetPlayer(LabelText)
	UpdateTarget(LabelTarget)
end)

--ANIMATIONS

Chose_Animation_Button2.MouseButton1Click:Connect(function()
	Animations_Section.Visible = false
	Dancing_Section.Visible = false
	Moving_Section.Visible = true
end)

Chose_Animation_Button1.MouseButton1Click:Connect(function()
	Animations_Section.Visible = false
	Moving_Section.Visible = false
	Dancing_Section.Visible = true
end)

VampireAnim_Button.MouseButton1Click:Connect(function()
	if game.Players.LocalPlayer.Character.Humanoid.RigType ~= Enum.HumanoidRigType.R15 then
		SendNotify("System VR7","يجب ان تكون R15",7)
		return
	end
	local Animate = plr.Character.Animate
	Animate.Disabled = true
	StopAnim()
	Animate.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=1083445855"
	Animate.idle.Animation2.AnimationId = "http://www.roblox.com/asset/?id=1083450166"
	Animate.walk.WalkAnim.AnimationId = "http://www.roblox.com/asset/?id=1083473930"
	Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=1083462077"
	Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=1083455352"
	Animate.climb.ClimbAnim.AnimationId = "http://www.roblox.com/asset/?id=1083439238"
	Animate.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=1083443587"
	plr.Character.Humanoid:ChangeState(3)
	Animate.Disabled = false
end)

