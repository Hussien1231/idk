--TARGET
ClickTargetTool_Button.MouseButton1Click:Connect(function()
    for _,P in ipairs(plr.Backpack:GetChildren()) do if P.Name == "ClickTarget" then P:Destroy() end end
	for _,P in ipairs(plr.Character:GetChildren()) do if P.Name == "ClickTarget" then P:Destroy() end end
	local GetTargetTool = Instance.new("Tool")
	GetTargetTool.Name = "ClickTarget"
	GetTargetTool.RequiresHandle = false
	GetTargetTool.TextureId = "rbxassetid://13769558274"
	GetTargetTool.ToolTip = "اختار شخص"

	local function ActivateTool()
		local root = GetRoot(plr)
		local hit = mouse.Target
		local person = nil
		if hit and hit.Parent then
			if hit.Parent:IsA("Model") then
				person = game.Players:GetPlayerFromCharacter(hit.Parent)
			elseif hit.Parent:IsA("Accessory") then
				person = game.Players:GetPlayerFromCharacter(hit.Parent.Parent)
			end
			if person then
				UpdateTarget(person)
			end
		end
	end

	GetTargetTool.Activated:Connect(function()
		ActivateTool()
	end)
	GetTargetTool.Parent = plr.Backpack
end)

FlingTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(FlingTarget_Button)
		FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BenxTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)

		if FlingTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			if game.PlaceId == 11379739543 then SendNotify("V R 7","هاذ الماب محمي من هذه الاوامر",3) FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) return end
			game:GetService("StarterGui"):SetCore("SendNotification", {Title = "System VR7",Text = "اذا ماطار الاعب معناها الماب فيه حماية",Duration = 3;})

			
			if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Humanoid and game.Players.LocalPlayer.Character.Humanoid.RootPart then
				if game.Players.LocalPlayer.Character.Humanoid.RootPart.Velocity.Magnitude < 50 then
					getgenv().OldPos = game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame
				end
				if Players[TargetedPlayer].Character.Head then
					workspace.CurrentCamera.CameraSubject = Players[TargetedPlayer].Character.Head
				elseif Players[TargetedPlayer].Character:FindFirstChildOfClass("Accessory"):FindFirstChild("Handle") then
					workspace.CurrentCamera.CameraSubject = Players[TargetedPlayer].Character:FindFirstChildOfClass("Accessory"):FindFirstChild("Handle")
				else
					workspace.CurrentCamera.CameraSubject = Players[TargetedPlayer].Character.Humanoid
				end
				if not Players[TargetedPlayer].Character:FindFirstChildWhichIsA("BasePart") then
					return
				end
				
				local function FPos(BasePart, Pos, Ang)
					game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(BasePart.Position) * Pos * Ang
					game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(BasePart.Position) * Pos * Ang)
					game.Players.LocalPlayer.Character.Humanoid.RootPart.Velocity = Vector3.new(9e7, 9e7 * 10, 9e7)
					game.Players.LocalPlayer.Character.Humanoid.RootPart.RotVelocity = Vector3.new(9e8, 9e8, 9e8)
				end
				
				local function SFBasePart()
					local Angle = 0
					getgenv().FPDH = workspace.FallenPartsDestroyHeight
					workspace.FallenPartsDestroyHeight = 0/0
					repeat
						task.wait()
						pcall(function()
							if game.Players.LocalPlayer.Character.Humanoid.RootPart and Players[TargetedPlayer].Character.Humanoid then
								if Players[TargetedPlayer].Character.Humanoid.RootPart.Velocity.Magnitude < 50 then
									Angle = Angle + 100
									for _, Offset in ipairs({
										Vector3.new(0, 1.5, 0), Vector3.new(0, -1.5, 0),
										Vector3.new(2.25, 1.5, -2.25), Vector3.new(-2.25, -1.5, 2.25),
										Vector3.new(0, 1.5, 0), Vector3.new(0, -1.5, 0)
									}) do
										FPos(Players[TargetedPlayer].Character.Humanoid.RootPart, CFrame.new(Offset) + Players[TargetedPlayer].Character.Humanoid.MoveDirection * (Players[TargetedPlayer].Character.Humanoid.RootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(Angle), 0, 0))
										task.wait()
									end
								else
									for _, Data in ipairs({
										{Vector3.new(0, 1.5, Players[TargetedPlayer].Character.Humanoid.WalkSpeed), math.rad(90)},
										{Vector3.new(0, -1.5, -Players[TargetedPlayer].Character.Humanoid.WalkSpeed), 0},
										{Vector3.new(0, 1.5, Players[TargetedPlayer].Character.Humanoid.WalkSpeed), math.rad(90)},
										{Vector3.new(0, 1.5, Players[TargetedPlayer].Character.Humanoid.RootPart.Velocity.Magnitude / 1.25), math.rad(90)},
										{Vector3.new(0, -1.5, -Players[TargetedPlayer].Character.Humanoid.RootPart.Velocity.Magnitude / 1.25), 0},
										{Vector3.new(0, 1.5, Players[TargetedPlayer].Character.Humanoid.RootPart.Velocity.Magnitude / 1.25), math.rad(90)},
										{Vector3.new(0, -1.5, 0), math.rad(90)},
										{Vector3.new(0, -1.5, 0), 0},
										{Vector3.new(0, -1.5, 0), math.rad(-90)},
										{Vector3.new(0, -1.5, 0), 0}
									}) do
										FPos(Players[TargetedPlayer].Character.Humanoid.RootPart, CFrame.new(Data[1]), CFrame.Angles(Data[2], 0, 0))
										task.wait()
									end                        
								end
								game.Players.LocalPlayer.Character.Humanoid.Sit = false
								if Players[TargetedPlayer].Character:FindFirstChild("Head") then
									workspace.CurrentCamera.CameraSubject = Players[TargetedPlayer].Character.Head
								end
							end
						end)
					until FlingTarget_Button.Ticket_Asset.ImageColor3 ~= Color3.fromRGB(0,255,0)
				end
				
				local BV = Instance.new("BodyVelocity")
				BV.Name = "Flinger"
				BV.Parent = game.Players.LocalPlayer.Character.Humanoid.RootPart
				BV.Velocity = Vector3.new(9e8, 9e8, 9e8)
				BV.MaxForce = Vector3.new(1/0, 1/0, 1/0)

				game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
			
				SFBasePart()

				BV:Destroy()
				game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
				workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
				
				repeat
					game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = getgenv().OldPos * CFrame.new(0, .5, 0)
					game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(getgenv().OldPos * CFrame.new(0, .5, 0))
					game.Players.LocalPlayer.Character.Humanoid:ChangeState("GettingUp")
					table.foreach(game.Players.LocalPlayer.Character:GetChildren(), function(_, x)
						if x:IsA("BasePart") then
							x.Velocity, x.RotVelocity = Vector3.new(), Vector3.new()
						end
					end)
					task.wait()
				until (game.Players.LocalPlayer.Character.Humanoid.RootPart.Position - getgenv().OldPos.p).Magnitude < 25
				workspace.FallenPartsDestroyHeight = getgenv().FPDH
				if game.Players.LocalPlayer.Character.Humanoid.Sit then
					wait(1)
					game.Players.LocalPlayer.Character.Humanoid.sit = false
				end
			end
		end
	end
end)

FuckYourSelf_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer ~= nil then
		ChangeToggleColor(FuckYourSelf_Button)
		if FuckYourSelf_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			if game.PlaceId == 11379739543 then SendNotify("V R 7","هاذ الماب محمي من هذه الاوامر",3) FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) return end
			for _, Track in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
				Track:Stop()
			end
			FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			BenxTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)

            local PositionValue = -2
			local increment = 0.1
            local Hight = -1
			repeat
                pcall(function()
                    local OtherTorso = Players[TargetedPlayer].Character:FindFirstChild("Torso") or Players[TargetedPlayer].Character:FindFirstChild("UpperTorso")
                    if OtherTorso.Name == "Torso" then Hight = -0.75 end
                    plr.Character.Humanoid.Sit = true
                    GetRoot(plr).CFrame = OtherTorso.CFrame * CFrame.new(0, Hight, PositionValue) * CFrame.Angles(math.rad(270), 0, 0)
                    GetRoot(plr).Velocity = Vector3.new(0, 0, 0)
                    PositionValue = PositionValue + increment
                    if PositionValue >= -1 or PositionValue <= -2 then
						increment = -increment
					end
                end)
                task.wait()
            until FuckYourSelf_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255, 0, 0)            
			GetRoot(plr).Parent.Humanoid.Sit = false
		end
	end
end)

