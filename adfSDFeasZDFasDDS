    if game:GetService("Players").LocalPlayer.Character and game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid", true) then
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid", true).HipHeight = 0.01
    end
