function CreateRainbowTextLabel(Parent, Text, Position, Rotation)
    local RainbowText = Instance.new("TextLabel", Parent)
    RainbowText.Text = Text
    RainbowText.Size = UDim2.new(0, 60, 0, 30)
    RainbowText.Position = Position
    RainbowText.TextColor3 = Color3.fromRGB(85, 255, 127)
    RainbowText.TextScaled = true
    RainbowText.Font = Enum.Font.SourceSans
    RainbowText.TextSize = 20
    RainbowText.TextTransparency = 0
    RainbowText.BackgroundTransparency = 1
    RainbowText.TextStrokeTransparency = 0.8
    RainbowText.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
    RainbowText.Rotation = Rotation
    return RainbowText
end

function CreateButtonWithText(Parent, ButtonName, ButtonText, Position, RainbowText, Rotation)
    local Button = Instance.new("TextButton", Parent)
    Button.Name = ButtonName
    Button.BackgroundColor3 = Color3.fromRGB(r, g, b)
    Button.BackgroundTransparency = 0.500
    Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Button.BorderSizePixel = 0
    Button.Position = Position
    Button.Size = UDim2.new(0, 150, 0, 30)
    Button.Font = Enum.Font.Oswald
    Button.Text = ButtonText
    Button.TextColor3 = Color3.fromRGB(0, 0, 0)
    Button.TextScaled = true
    Button.TextSize = 14
    Button.TextWrapped = true
    if ButtonName == "VehicleFling_Button" or ButtonName == "Ball_Fling" or ButtonName == "Spam_Skins" or ButtonName == "Spamming_Commands" then
        CreateToggle(Button)
    else
        CreateClicker(Button)
    end
    return {t1= CreateRainbowTextLabel(Parent, RainbowText, UDim2.new(0, Position.X.Offset - 15 , 0, Position.Y.Offset + 15), Rotation),t2 = Button}
end

if game.PlaceId == 14201103742 or game.PlaceId == 12123568130 or game.PlaceId == 18209796679 or game.PlaceId == 17723040340 or game.PlaceId == 88950799188393 then
	pcall(function()
	local Workspace = game:GetService("Workspace")
	local RunService = game:GetService("RunService")

	local LocalPlayer = game.Players.LocalPlayer
	local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

	local Folder = Instance.new("Folder", Workspace)
	local Part = Instance.new("Part", Folder)
	local Attachment1 = Instance.new("Attachment", Part)
	Part.Anchored = true
	Part.CanCollide = false
	Part.Transparency = 1
	if not getgenv().Network then
		getgenv().Network = {
			BaseParts = {},
			Velocity = Vector3.new(14.46262424, 14.46262424, 14.46262424)
		}

		Network.RetainPart = function(Part)
			if typeof(Part) == "Instance" and Part:IsA("BasePart") and Part:IsDescendantOf(Workspace) and not Part:IsDescendantOf(game.Players.LocalPlayer.Character) then
				table.insert(Network.BaseParts, Part)
				Part.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
				Part.CanCollide = false
			end
		end
		
		local function EnablePartControl()
			LocalPlayer.ReplicationFocus = Workspace
			RunService.Heartbeat:Connect(function()
				pcall(function()
					sethiddenproperty(LocalPlayer, "SimulationRadius", math.huge)
					for _, Part in pairs(Network.BaseParts) do
						if Part:IsDescendantOf(Workspace) then
							Part.Velocity = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 50
						end
					end
				end)
			end)
		end
		EnablePartControl()
	end
		function Flinger(v)
			if getgenv().loopfling and v:IsA("BasePart") and not v.Anchored and not v.Parent:FindFirstChild("Humanoid") and not v.Parent:FindFirstChild("Head") and v.Name ~= "Handle" then
				local OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
				wait(0.7)
				for _, x in next, v:GetChildren() do
					if x:IsA("BodyAngularVelocity") or x:IsA("BodyForce") or x:IsA("BodyGyro") or x:IsA("BodyPosition") or x:IsA("BodyThrust") or x:IsA("BodyVelocity") or x:IsA("RocketPropulsion") then
						x:Destroy()
					end
				end
				if v:FindFirstChild("Attachment") then
					v:FindFirstChild("Attachment"):Destroy()
				end
				if v:FindFirstChild("AlignPosition") then
					v:FindFirstChild("AlignPosition"):Destroy()
				end
				if v:FindFirstChild("Torque") then
					v:FindFirstChild("Torque"):Destroy()
				end
				v.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
				v.CanCollide = false
				v.CanTouch = false
				local Torque = Instance.new("Torque", v)
				Torque.Torque = Vector3.new(100000, 100000, 100000)
				local AlignPosition = Instance.new("AlignPosition", v)
				local Attachment2 = Instance.new("Attachment", v)
				Torque.Attachment0 = Attachment2
				AlignPosition.MaxForce = 99999999999999999
				AlignPosition.MaxVelocity = math.huge
				AlignPosition.Responsiveness = 200
				AlignPosition.Attachment0 = Attachment2
				AlignPosition.Attachment1 = Attachment1
				spawn(function()
					repeat task.wait()
						if getgenv().loopfling then
							pcall(function()
								if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
									getgenv().look = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0,-1,0)
								end
							end)
						end
					until getgenv().ejnfja 
				end)
				spawn(function()
					v.CFrame = humanoidRootPart.CFrame
					local Angle = 0
					if not getgenv().working then
						while true do
							getgenv().working = true
							task.wait()
							pcall(function()
								Angle = Angle + math.rad(60)
								local Offset = CFrame.new(math.sin(Angle) * 3, 0, math.cos(Angle) * 3)
								Attachment1.WorldCFrame = getgenv().look * Offset
								v.CFrame = getgenv().look * Offset
								v.Velocity = getgenv().look.LookVector * 10000
							end)
						end
					end
				end)
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
			end  
		end
	end)
end
function LoadChatAlert()
    local TextServiceGeneral = game:GetService("TextChatService"):FindFirstChild("RBXGeneral",true)

		local ChatAlert_Button = Instance.new("TextButton")
		ChatAlert_Button.Name = "ChatAlert_Button"
		ChatAlert_Button.Parent = Game_Section
		ChatAlert_Button.BackgroundColor3 = Color3.fromRGB(r, g, b)
		ChatAlert_Button.BackgroundTransparency = 0.500
		ChatAlert_Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
		ChatAlert_Button.BorderSizePixel = 0
		ChatAlert_Button.Position = UDim2.new(0, 25, 0, 175)
		ChatAlert_Button.Size = UDim2.new(0, 150, 0, 30)
		ChatAlert_Button.Font = Enum.Font.Oswald
        if TextServiceGeneral then
            ChatAlert_Button.Text = "تجسس الرسائل"
        else
		    ChatAlert_Button.Text = "تنبيه الدردشة "
        end
		ChatAlert_Button.TextColor3 = Color3.fromRGB(0, 0, 0)
		ChatAlert_Button.TextScaled = true
		ChatAlert_Button.TextSize = 14.000
		ChatAlert_Button.TextWrapped = true
		CreateClicker(ChatAlert_Button)
		ChatAlert_Button.MouseButton1Click:Connect(function()
        if TextServiceGeneral then
            loadstring(game:HttpGet('https://raw.githubusercontent.com/Hm5011/hussain/refs/heads/main/Chat%20Spy'))()
        else
			Send("hello                                                                                                              You must wait before sending another message.")
        end
    end)
end
