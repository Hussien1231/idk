	end
	wait(100)
end)
local ConfigPath = "workspace/VR7Settings.txt"

if not isfolder("workspace") then
    makefolder("workspace")
end

local DefaultConfig = {
    NotificationMute = false,
    AutoExecute = false,
    BangSpeed = "2",
}

function SaveConfig(Data)
    writefile(ConfigPath, game:GetService("HttpService"):JSONEncode(Data))
end

function LoadConfig()
    local Data

    if isfile(ConfigPath) then
        local Raw = readfile(ConfigPath)
        local Decoded

        pcall(function()
            Decoded = game:GetService("HttpService"):JSONDecode(Raw)
        end)

        if type(Decoded) ~= "table" then
            SaveConfig(DefaultConfig)
            return DefaultConfig
        end

        for Key in pairs(DefaultConfig) do
            if Decoded[Key] == nil then
                SaveConfig(DefaultConfig)
                return DefaultConfig
            end
        end

        return Decoded
    else
        SaveConfig(DefaultConfig)
        return DefaultConfig
    end
end


getgenv().ConfigData = LoadConfig()
getgenv().NotifcationVloume = 10
if getgenv().ConfigData.NotificationMute then 
	getgenv().NotifcationVloume = 0
end
