	Home_Section.Visible = false
	getgenv().Settings_Button_Section.Visible = true
end)

getgenv().MuteNoti.MouseButton1Click:Connect(function()
ChangeToggleColor(getgenv().MuteNoti)
	if getgenv().MuteNoti.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		getgenv().NotifcationVloume = 0
		getgenv().ConfigData.NotificationMute = true
	else
		getgenv().NotifcationVloume = 10
		getgenv().ConfigData.NotificationMute = false
	end
	SaveConfig(getgenv().ConfigData)
end)

getgenv().ChangeColor.MouseButton1Click:Connect(function()
    local colors = Colors()
    local r, g, b = colors.r, colors.g, colors.b
    local r2, g2, b2 = colors.r2, colors.g2, colors.b2

    getgenv().ColorforFback = Color3.fromRGB(r, g, b)
    local primary = Color3.fromRGB(r, g, b)
    local secondary = Color3.fromRGB(r2, g2, b2)

    local Sections = getgenv().colorSections
    local Gui = getgenv().VR7

    local function classify(color)
        for _, s in ipairs(Sections) do
            if color == Color3.fromRGB(s.r, s.g, s.b) then return "primary" end
            if color == Color3.fromRGB(s.r2, s.g2, s.b2) then return "secondary" end
        end
    end

    for _, obj in ipairs(Gui:GetDescendants()) do
        local function trySetColor(propName, newColor)
            local success, _ = pcall(function() obj[propName] = newColor end)
            return success
        end

        local ok, col = pcall(function() return obj.BackgroundColor3 end)
        if ok and col then
            local t = classify(col)
            if t == "primary" then trySetColor("BackgroundColor3", primary)
            elseif t == "secondary" then trySetColor("BackgroundColor3", secondary)
            end
        end

        ok, col = pcall(function() return obj.TextColor3 end)
        if ok and col then
            local t = classify(col)
            if t == "primary" then trySetColor("TextColor3", primary)
            elseif t == "secondary" then trySetColor("TextColor3", secondary)
            end
        end

        if (obj:IsA("ImageLabel") or obj:IsA("ImageButton")) and
           obj.Name ~= "Profile_Image" and
           obj.Name ~= "TargetImage" and
		   obj.Name ~= "ClickTargetTool_Button" and
           obj.Name ~= "Ticket_Asset" then
            ok, col = pcall(function() return obj.ImageColor3 end)
            if ok and col then
                local t = classify(col)
                if t == "primary" then trySetColor("ImageColor3", primary)
                elseif t == "secondary" then trySetColor("ImageColor3", secondary)
                end
            end
        end

        ok, col = pcall(function() return obj.BorderColor3 end)
        if ok and col then
            local t = classify(col)
            if t == "primary" then trySetColor("BorderColor3", primary)
            elseif t == "secondary" then trySetColor("BorderColor3", secondary)
            end
        end

        if obj:IsA("UIStroke") then
            local t = classify(obj.Color)
            if t == "primary" then
                pcall(function() obj.Color = primary end)
            elseif t == "secondary" then
                pcall(function() obj.Color = secondary end)
            end
        end
    end
end)


if getgenv().ConfigData.NotificationMute then
	getgenv().MuteNoti.Ticket_Asset.ImageColor3 = Color3.fromRGB(0,255,0)
end

getgenv().HelpHd_Button.MouseButton1Click:Connect(function()
	SendNotify("System VR7","خانة الاوامر تستخدم لكتابة اوامر ادمن بدون احد يشوفك",5)
	local sound = Instance.new("Sound", game.Workspace) sound.SoundId = "rbxassetid://3398620867" sound.Volume = getgenv().NotifcationVloume sound.Ended:Connect(function() sound:Destroy() end) sound:Play()
end)

Noclip_Button.MouseButton1Click:Connect(function()
	ChangeToggleColor(Noclip_Button)
	if Noclip_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		local trueparts = {}
		for _,P in ipairs(GetRoot(plr).Parent:GetChildren()) do 
			if P:IsA("BasePart") then
				table.insert(trueparts,P)
			end
		end
		repeat wait(0.1)
			pcall(function()
				for _,P in ipairs(trueparts) do 
					if plr.Character[P.Name].CanCollide then task.wait()
					plr.Character[P.Name].CanCollide = false
					end
				end
			end)
		until Noclip_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255, 0, 0)		
		for _,P in ipairs(trueparts) do 
			if not plr.Character[P.Name].CanCollide then
				task.wait(0.01)
				plr.Character[P.Name].CanCollide = true
			end
		end
		game.Players.LocalPlayer.Character.Humanoid:ChangeState("GettingUp")
	end
end)

SaveCheckpoint_Button.MouseButton1Click:Connect(function()
	SavedCheckpoint = GetRoot(plr).Position
	SendNotify("System VR7","تم حفظ الشيك بوينت",5)
end)

ClearCheckpoint_Button.MouseButton1Click:Connect(function()
	SavedCheckpoint = nil
	SendNotify("System VR7","تم حذف الشيك بوينت",5)
end)

local flying = true
local deb = true
local ctrl = {f = 0, b = 0, l = 0, r = 0}
local lastctrl = {f = 0, b = 0, l = 0, r = 0}
local KeyDownFunction = nil
local KeyUpFunction = nil
Fly_Button.MouseButton1Click:Connect(function()
	if game.PlaceId == 11379739543 then SendNotify("V R 7","هاذ الماب محمي من هذه الاوامر",3) Fly_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) return end
	ChangeToggleColor(Fly_Button)
	if Fly_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		flying = true
		if game:GetService("UserInputService").TouchEnabled then
			Fly_Pad.Visible = true
		end
		local Torso = plr.Character:FindFirstChild("UpperTorso") or plr.Character:FindFirstChild("Torso")
		local speed = 0
		local function Fly()
			local bg = Instance.new("BodyGyro", Torso)
			bg.P = 9e4
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
			bg.cframe = Torso.CFrame
			local bv = Instance.new("BodyVelocity", Torso)
			bv.velocity = Vector3.new(0,0.1,0)
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
			PlayAnim(10714347256,4,0)
			repeat task.wait()
				plr.Character.Humanoid.PlatformStand = true
				if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then
					speed = speed+FlySpeed*0.10
					if speed > FlySpeed then
						speed = FlySpeed
					end
				elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then
					speed = speed-FlySpeed*0.10
					if speed < 0 then
						speed = 0
					end
				end
				if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed
					lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r}
				elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed
				else
					bv.velocity = Vector3.new(0,0.1,0)
				end
				bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/FlySpeed),0,0)
			until not flying
			ctrl = {f = 0, b = 0, l = 0, r = 0}
			lastctrl = {f = 0, b = 0, l = 0, r = 0}
			speed = 0
			bg:Destroy()
			bv:Destroy()
			plr.Character.Humanoid.PlatformStand = false
		end

		KeyDownFunction = mouse.KeyDown:connect(function(key)
			if key:lower() == "w" then
				ctrl.f = 1
				if plr.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then PlayAnim(10714177846,4.65,0) else PlayAnim(282574440,4.65,0) end 
			elseif key:lower() == "s" then
				ctrl.b = -1
				PlayAnim(10147823318,4.11,0)
			elseif key:lower() == "a" then
				ctrl.l = -1
				PlayAnim(10147823318,3.55,0)
			elseif key:lower() == "d" then
				ctrl.r = 1
				PlayAnim(10147823318,4.81,0)
			end
		end)

		KeyUpFunction = mouse.KeyUp:connect(function(key)
			if key:lower() == "w" then
				ctrl.f = 0
				PlayAnim(10714347256,4,0)
			elseif key:lower() == "s" then
				ctrl.b = 0
				PlayAnim(10714347256,4,0)
			elseif key:lower() == "a" then
				ctrl.l = 0
				PlayAnim(10714347256,4,0)
			elseif key:lower() == "d" then
				ctrl.r = 0
				PlayAnim(10714347256,4,0)
			end
		end)
		Fly()
	else
		flying = false
		Fly_Pad.Visible = false
		KeyDownFunction:Disconnect()
		KeyUpFunction:Disconnect()
		StopAnim()
	end
end)

