ChatSpam_Button.MouseButton1Click:Connect(function()
	ChangeToggleColor(ChatSpam_Button)
	if ChatSpam_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		repeat wait(.15)
            Send(Chatted)
        until ChatSpam_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
	end
end)

ChatMissing_Button.MouseButton1Click:Connect(function()
	if game:GetService("TextChatService").ChatVersion == Enum.ChatVersion.TextChatService then
		local SafeTimer = 0
		repeat task.wait()
			SafeTimer += 1
				Send("هاي\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r.")
		until SafeTimer >= 3
		SendNotify("System VR7","انتبه اذا تسوي سبام كثير احتمال يجيك تحذير",5)
	else
		SendNotify("System VR7","هاذ النوع من الشات غير مدعوم بسبب وجود حمايه",5)
	end
end)

Jerking_Button.MouseButton1Click:Connect(function()
	local G = loadstring(game:HttpGet("https://raw.githubusercontent.com/Bwhw827g29wh/Scripts/refs/heads/main/FetchAndExecute.lua"))()
    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    if char then
        local humanoid = char:FindFirstChildOfClass("Humanoid")

        if humanoid and humanoid.RigType == Enum.HumanoidRigType.R15 then
            G:fetchAndExecute("https://pastefy.app/YZoglOyJ/raw") 
        else
            G:fetchAndExecute("https://pastefy.app/wa3v2Vgm/raw") 
        end
    end
end)
CMDBar.FocusLost:Connect(function()
	command = CMDBar.Text
    Execute(command)
	CMDBar.Text = ""
end)

ChatBar.FocusLost:Connect(function()
	Chatted = ChatBar.Text
end)

--CHARACTER SECTION

WalkSpeed_Button.MouseButton1Click:Connect(function()
	if game.PlaceId == 11379739543 then
		if getgenv().Tpwalking then
			getgenv().Tpwalking = false
			SendNotify("System VR7","اضغط مرة ثانية للتسريع",5)
		elseif not getgenv().Tpwalking then
			getgenv().Tpwalking = true
			SendNotify("System VR7","اضغط مرة ثانية لارجاع السرعة الطبيعية",5)
		end
		local Speed = WalkSpeed_Input.Text:gsub("%D", "")
			if Speed == "" then
				Speed = 1
			elseif tonumber(Speed) > 10 then
				Speed = 10
			end
		while getgenv().Tpwalking do task.wait()
			pcall(function()
				local delta = game:GetService("RunService").Heartbeat:Wait()
				if game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Magnitude > 0 then
					game.Players.LocalPlayer.Character:TranslateBy(game.Players.LocalPlayer.Character.Humanoid.MoveDirection * Speed * delta * 10)
				else
					game.Players.LocalPlayer.Character:TranslateBy(game.Players.LocalPlayer.Character.Humanoid.MoveDirection * delta * 10)
				end
			end)
		end
	else
		pcall(function()
			local Speed = WalkSpeed_Input.Text:gsub("%D", "")
			if Speed == "" then
				Speed = 16
			end
			plr.Character.Humanoid.WalkSpeed = tonumber(Speed)
			SendNotify("System VR7","تم تحديث السرعة",5)
		end)
	end
end)

JumpPower_Button.MouseButton1Click:Connect(function()
	pcall(function()
		local Power = JumpPower_Input.Text:gsub("%D", "")
		if Power == "" then
			Power = 50
		end
		plr.Character.Humanoid.JumpPower = tonumber(Power)
		SendNotify("System VR7","تم تحديث قوة القفز",5)
	end)
end)

