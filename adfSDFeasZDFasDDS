        if getgenv().RainbowTjm33.t2.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then getgenv().RainbowTjm33.t2.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) getgenv().Cuff = {Bring = false,Throw = false} wait(1) end
