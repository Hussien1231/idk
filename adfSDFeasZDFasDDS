                                    game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid").RootPart.Velocity.Y,
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid").MoveDirection.Z * 40
                                )
                        end
                    end
                end
