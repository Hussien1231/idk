getgenv().RainbowTjm35.t2.MouseButton1Click:Connect(function()
	pcall(function()
		if TargetedPlayer ~= nil then
			local Cuff = GetCuff()
			if not Cuff then
				SendNotify("System VR7","لازم يكون معك كلبشة",5) return 
			elseif Players[TargetedPlayer].Character.UpperTorso.Anchored then
				SendNotify("System VR7","الاعب متجمد او مكلبش بل اساس",5) return 
			end 
			if Cuff and Cuff.Parent == game.Players.LocalPlayer.Backpack then Cuff.Parent = game.Players.LocalPlayer.Character end
			wait()
			Cuff.RemoteEvent:FireServer(Players[TargetedPlayer].Character.HumanoidRootPart, "Cuff")
			wait(0.2)
			Cuff.RemoteEvent:FireServer(Players[TargetedPlayer].Character.HumanoidRootPart, "Cuff")
		end
	end)
end)

getgenv().RainbowTjm41.t2.MouseButton1Click:Connect(function()
	ChangeToggleColor(getgenv().RainbowTjm41.t2)
	if getgenv().RainbowTjm41.t2.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		while getgenv().RainbowTjm41.t2.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) do 
			if game:GetService("TextChatService").ChatVersion == Enum.ChatVersion.TextChatService then
				if game:GetService("TextChatService").ChatVersion == Enum.ChatVersion.TextChatService then
				local SafeTimer = 0
					repeat task.wait(0.3)
					SafeTimer += 1
					Send("هاي\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r\r.")
					until SafeTimer >= 8
				end
				task.wait(0.5)
			end
		end
	end
end)

