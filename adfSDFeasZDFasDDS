getgenv().RainbowTjm31.t2.MouseButton1Click:Connect(function()
    if firesignalR then
        ChangeToggleColor(getgenv().RainbowTjm31.t2)
        local Names = {"الي قدامي وصخ","انا عمك","تاج راسك","بنعالي شالية روب","مكافحة الرول","ضاغطهم🔥","تخريب على الرول","بنعالي صاحب الماب ومشرفينه","افضل هاك ايفون اختصاره دسكورد vr7","رجلي فيك","يلا انبح","خليك مضغوط مني","دز امها بس","الي قدامي كلب","بنعالي كل الي ضاغطهم ✌️","VR7 عمك","انا فحل الماب","منبع المرجله هنا"}    
        local ChangeNameButton = game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Title.ChangeName
        local NameTextBox = game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Title.Input
    
        while getgenv().RainbowTjm31.t2.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) and task.wait(5) do
            local RandomName = Names[math.random(1, #Names)]
            local ColorsNumber = math.random(1, #game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Title.Colors:GetChildren())
            NameTextBox.Text = RandomName
            for i, P in ipairs(game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Title.Colors:GetChildren()) do 
                if i == ColorsNumber and P:IsA("Frame") then
                    firesignal(P["hit_box"].MouseButton1Click)
                    break
                end
            end
            firesignal(ChangeNameButton.MouseButton1Click)
            if game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.HiddenCommands.Visible then
                game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.HiddenCommands.Visible = false
            end
        end
    else
        SendNotify("System VR7","للاسف الهاك الي تستخدمة لايدعم هل ميزة",5)
    end
end)

getgenv().RainbowTjm33.t2.MouseButton1Click:Connect(function()
    if GetCuff() then
	ChangeToggleColor(getgenv().RainbowTjm33.t2)
    else game:GetService("StarterGui"):SetCore("SendNotification", {Title = "System VR7",Text = "لازم يكون معك كلبشة",Duration = 5;}) return end 
	if getgenv().RainbowTjm33.t2.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
        if getgenv().RainbowTjm34.t2.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then getgenv().RainbowTjm34.t2.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) getgenv().Cuff = {Bring = false,Throw = false} wait(1) end
        getgenv().Cuff = {Bring = true,Throw = true}
        Cuffbring()
    else
        getgenv().Cuff = {Bring = false,Throw = false}
	end
end)

