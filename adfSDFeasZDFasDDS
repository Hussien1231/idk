		SendNotify("System VR7","يجب ان تكون R15",7)
		return
	end
	getgenv().idkjustcheck = false
	local Track
	local Animation = Instance.new("Animation")
	Animation.AnimationId = "rbxassetid://13694096724"
	for _, Track in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
		if Track.Animation.AnimationId == Animation.AnimationId then
			Track:Stop()
			getgenv().idkjustcheck = true
		else
			Track:Stop()
		end
	end
	if getgenv().idkjustcheck then getgenv().idkjustcheck = false return end 
	local Humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
	Track = Humanoid:LoadAnimation(Animation)
	Track.Priority = Enum.AnimationPriority.Action
	Track.Looped = false
	Track:Play()
	Track.TimePosition = 3.45
	Track:AdjustSpeed(0)
end)

