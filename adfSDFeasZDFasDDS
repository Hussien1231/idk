function ChangeSection(SectionClicked)
	SectionClickedName = string.split(SectionClicked.Name,"_")[1]
	for i,v in pairs(SectionList:GetChildren()) do
		if v.Name ~= SectionClicked.Name then
			v.Transparency = 0.5
		else
			v.Transparency = 0
		end
	end
	for i,v in pairs(Background:GetChildren()) do
		if v:IsA("ScrollingFrame") then
			SectionForName = string.split(v.Name,"_")[1]
			if string.find(SectionClickedName, SectionForName) then
				v.Visible = true
			else
				v.Visible = false
			end
		end
	end
end

--local Whitelisted = {"8233108521","4542242689","8145979278","5350231752","951402104","2393639720","7610949712","1783292590","8334377717","7551627155","2824436628","5837426408","7873156557","697531197","7552480459"}
function UpdateTarget(player)
	pcall(function()
	if player.UserId == 951402104 then
		game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "VR7 Team",
				Text = "اجراء خاطئ لان راعي\n  الحساب تحت حماية حسوني",
				Duration = 5,
				Icon = "rbxassetid://6710742632",
			})
			local sound = Instance.new("Sound", game.Workspace) sound.SoundId = "rbxassetid://17692186249" sound.Volume = getgenv().NotifcationVloume sound.Ended:Connect(function() sound:Destroy() end) sound:Play()
			player = nil
	elseif table.find(loadstring(game:HttpGet('https://raw.githubusercontent.com/Hm5011/hussain/refs/heads/main/Hehehe'))(),tostring(player.UserId)) or table.find(loadstring(game:HttpGet('https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/Usernames'))(),tostring(player.UserId)) then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "VR7 Team",
				Text = "لايمكنك اختيار هذا الاعب\n لانه محمي من صاحب السكربت",
				Duration = 5,
				Icon = "rbxassetid://136772242182146",
			})
			local sound = Instance.new("Sound", game.Workspace) sound.SoundId = "rbxassetid://1862047553" sound.Volume = getgenv().NotifcationVloume sound.Ended:Connect(function() sound:Destroy() end) sound:Play()
			player = nil
		end
	end)
	if (player ~= nil) then
		TargetedPlayer = player.Name
		TargetName_Input.Text = player.Name
		UserIDTargetLabel.Text = ("UserID: "..player.UserId.."\nDisplay: "..player.DisplayName.."\nJoined: "..os.date("%d-%m-%Y", os.time()-player.AccountAge * 24 * 3600).." [سنة/شهر/يوم]")
		TargetImage.Image = Players:GetUserThumbnailAsync(player.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size420x420)
		RepeatTalk_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		if TalkRepeater then TalkRepeater:Disconnect() end
        FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
	else
		TargetName_Input.Text = "@target..."
		UserIDTargetLabel.Text = "UserID: \nDisplay: \nJoined: "
		TargetImage.Image = "rbxassetid://10818605405"
		TargetedPlayer = nil
		if FlingTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			FlingTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
			ChatSpam_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		end
        FuckYourSelf_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		ViewTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		FocusTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BenxTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		HeadsitTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		StandTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		BackpackTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		JerkOnTarget_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		RepeatTalk_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		if TalkRepeater then TalkRepeater:Disconnect() end
		pcall(function() getgenv().AdminSpamming.t2.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) end)
		pcall(function() if game.PlaceId == 11379739543 then getgenv().RainbowTjm3.t2.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) getgenv().RainbowTjm33.t2.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0) end end)
	end
end

