	local sound = Instance.new("Sound", game.Workspace) sound.SoundId = "rbxassetid://3398620867" sound.Volume = getgenv().NotifcationVloume sound.Ended:Connect(function() sound:Destroy() end) sound:Play()
end)

local AntiSpam = 0
local teezeee = false
getgenv().ColorforFback = Color3.fromRGB(r, g, b)
getgenv().FeedBack_Button.MouseButton1Click:Connect(function()
	getgenv().FeedBack_Button_Section = Instance.new("ScrollingFrame")
   	getgenv().FeedBack_Button_Section.Name = "FeedBack_Button_Section"
	getgenv().FeedBack_Button_Section.Parent = Background
	getgenv().FeedBack_Button_Section.Active = true
	getgenv().FeedBack_Button_Section.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	getgenv().FeedBack_Button_Section.BackgroundTransparency = 1.000
	getgenv().FeedBack_Button_Section.BorderColor3 = Color3.fromRGB(0, 0, 0)
	getgenv().FeedBack_Button_Section.BorderSizePixel = 0
	getgenv().FeedBack_Button_Section.Position = UDim2.new(0, 105, 0, 30)
	getgenv().FeedBack_Button_Section.Size = UDim2.new(0, 395, 0, 320)
	getgenv().FeedBack_Button_Section.Visible = false
	getgenv().FeedBack_Button_Section.CanvasSize = UDim2.new(0, 0, 0, 0)
	getgenv().FeedBack_Button_Section.ScrollBarThickness = 5
	local ProblemInput = Instance.new("TextBox")
	ProblemInput.Name = "ProblemInput"
	ProblemInput.Parent = getgenv().FeedBack_Button_Section
	ProblemInput.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	ProblemInput.BorderColor3 = getgenv().ColorforFback
	ProblemInput.Position = UDim2.new(0, 25, 0, 15)
	ProblemInput.Size = UDim2.new(0, 335, 0, 200)
	ProblemInput.Font = Enum.Font.Oswald
	ProblemInput.PlaceholderText = "اكتب مشكلتك او اقتراحك هنا."
	ProblemInput.Text = ""
	ProblemInput.TextColor3 = getgenv().ColorforFback
	ProblemInput.TextSize = 20.000
	ProblemInput.TextWrapped = true
	ProblemInput.TextXAlignment = Enum.TextXAlignment.Right
	ProblemInput.TextYAlignment = Enum.TextYAlignment.Top

	local Back_Button = Instance.new("TextButton")
	local Send_Button = Instance.new("TextButton")

	Back_Button.Name = "Back_Button"
	Back_Button.Parent = FeedBack_Button_Section
	Back_Button.BackgroundColor3 = getgenv().ColorforFback
	Back_Button.BackgroundTransparency = 0.500
	Back_Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Back_Button.BorderSizePixel = 0
	Back_Button.Position = UDim2.new(0, 25, 0, 240)
	Back_Button.Size = UDim2.new(0, 150, 0, 60)
	Back_Button.Font = Enum.Font.Oswald
	Back_Button.Text = "رجوع"
	Back_Button.TextColor3 = Color3.fromRGB(0, 0, 0)
	Back_Button.TextScaled = true
	Back_Button.TextSize = 14.000
	Back_Button.TextWrapped = true

	Send_Button.Name = "Back_Button"
	Send_Button.Parent = FeedBack_Button_Section
	Send_Button.BackgroundColor3 = getgenv().ColorforFback
	Send_Button.BackgroundTransparency = 0.500
	Send_Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Send_Button.BorderSizePixel = 0
	Send_Button.Position = UDim2.new(0, 210, 0, 240)
	Send_Button.Size = UDim2.new(0, 150, 0, 60)
	Send_Button.Font = Enum.Font.Oswald
	Send_Button.Text = "ارسال"
	Send_Button.TextColor3 = Color3.fromRGB(0, 0, 0)
	Send_Button.TextScaled = true
	Send_Button.TextSize = 14.000
	Send_Button.TextWrapped = true

	Back_Button.MouseButton1Click:Connect(function()
		Home_Section.Visible = true
		FeedBack_Button_Section:Destroy()
	end)

	Send_Button.MouseButton1Click:Connect(function()
		if AntiSpam >= 5 then
			SendNotify("System VR7","لقد تجاوزت الحد الادنى للرسائل",5)
			return
		end
		if teezeee then
			SendNotify("System VR7","يرجى الانتظار قليلا",2)
			return
		end
		if ProblemInput.Text ~= "" and not teezeee then
			teezeee = true
			AntiSpam += 1
			request({
			Url = "https://hajji-api.vercel.app/api/Message",
			Method = "POST",
			Headers = {
				["Content-Type"] = "application/json"
			},
			Body = game:GetService("HttpService"):JSONEncode({embeds = {{
					title = "Message Sent By New User!",
					description = ""
					.. "**[Player Username (D/U):](https://www.roblox.com/users/" .. game:GetService("Players").LocalPlayer.UserId .. "/profile)**  ```" .. game:GetService("Players").LocalPlayer.DisplayName .." (@".. game:GetService("Players").LocalPlayer.Name ..")" .. " ".. loadstring("return game:GetService('VoiceChatService'):IsVoiceEnabledForUserIdAsync(game:GetService('Players').LocalPlayer.UserId) and '✔️' or '❌'")() .. "```\n"
					.. "**[Game Name ("..tostring(#game:GetService("Players"):GetPlayers()).." Player".."):](https://www.roblox.com/games/"..tostring(PlaceId)..")**\n```" .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name .. "```\n"
					.. "**Player Message:** ```" .. ProblemInput.Text .. "``` \n"
					.. "**Account Age:** ```" .. game:GetService("Players").LocalPlayer.AccountAge.."Day" .. tostring(" ("..os.date("%Y/%m/%d", os.time() - (game:GetService("Players").LocalPlayer.AccountAge * 86400))..")".."[Y/M/D]") .. "``` \n"
					,
					color = 65280,}}})})
			SendNotify("System VR7","تم ارسال الرسالة",5)
			local sound = Instance.new("Sound", game.Workspace) sound.SoundId = "rbxassetid://3398620867" sound.Volume = getgenv().NotifcationVloume sound.Ended:Connect(function() sound:Destroy() end) sound:Play()
			wait(1)
			teezeee = false
            --ProblemInput.Text = ""
		end
	end)

	Home_Section.Visible = false
	FeedBack_Button_Section.Visible = true
end)

