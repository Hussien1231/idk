    Welcome_Label.Text = "مساء النور "..NameToUse.."@".."\nاضغط [B] لاخفاء الواجهة"
else
    Welcome_Label.Text = "صباح النور "..NameToUse.."@".."\nاضغط [B] لاخفاء الواجهة"
end
